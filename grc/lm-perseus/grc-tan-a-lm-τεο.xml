<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusτεο">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to τεο</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%84%CE%B5%CE%BF.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>τεο</tok-starts-with>
      <tok-starts-with>τέο</tok-starts-with>
      <tok-starts-with>τεό</tok-starts-with>
      <tok-starts-with>τεὸ</tok-starts-with>
      <ana><!--τεο τέο τεοῖο τεοῦ τεοῦς-->
         <tok val="τ[εέ]ο[ῦ]?(ς|ῖό|ῖο)?"/>
         <lm>
            <l>σύ</l>
            <m>p p - s - - - - g</m>
         </lm>
      </ana>
      <ana>
         <tok val="τεο"/>
         <lm>
            <l>τις</l>
            <m cert="0.5">p x - s - - - c g</m>
            <m cert="0.5">p x - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="τέο"/>
         <lm>
            <l>τίς</l>
            <m>p i - s - - - - g</m>
         </lm>
      </ana>
      <ana><!--τεοῖο τεοῦ-->
         <tok val="τεο[ῖ]?[όοῦ]"/>
         <lm>
            <l>τεός</l>
            <m cert="0.5">p s - s - - - m g</m>
            <m cert="0.5">p s - s - - - n g</m>
         </lm>
      </ana>
      <ana><!--τεοῖς τεοῖσι τεοῖσιν-->
         <tok val="τεοῖ[σ]?[ι]?(ν|ίν|ί|ς)?"/>
         <lm>
            <l>τεός</l>
            <m cert="0.5">p s - p - - - m d</m>
            <m cert="0.5">p s - p - - - n d</m>
         </lm>
      </ana>
      <ana>
         <tok val="τ[ὲέ]οισ[ὶί]"/>
         <lm>
            <l>τίς</l>
            <m cert="0.5">p i - p - - - m d</m>
            <m cert="0.5">p i - p - - - c d</m>
         </lm>
      </ana>
      <ana><!--τέοισί-->
         <tok val="τ[έὲ]οισ[ίὶ]"/>
         <lm>
            <l>τίς</l>
            <m>p i - p - - - n d</m>
         </lm>
      </ana>
      <ana><!--τεόν τεὸν-->
         <tok val="τε[ὸό]ν"/>
         <lm>
            <l>τεός</l>
            <m cert="0.25">p s - s - - - m a</m>
            <m cert="0.25">p s - s - - - n n</m>
            <m cert="0.25">p s - s - - - n v</m>
            <m cert="0.25">p s - s - - - n a</m>
         </lm>
      </ana>
      <ana><!--τεός τεὸς-->
         <tok val="τε[ὸό]ς"/>
         <lm>
            <l>τεός</l>
            <m>p s - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="τεοὺς"/>
         <lm>
            <l>τεός</l>
            <m>p s - p - - - m a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

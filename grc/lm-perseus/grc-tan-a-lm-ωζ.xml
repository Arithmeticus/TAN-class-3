      <for-lang>grc</for-lang>
      <tok-starts-with>ὤζ</tok-starts-with>
      <tok-starts-with>ᾠζ</tok-starts-with>
      <ana>
         <tok val="ὤζεις"/>
         <lm>
            <l>ὤζω</l>
            <m>v - 2 s p i a</m>
         </lm>
      </ana>
      <ana><!--ᾠζύρʼ ᾠζυρὰ-->
         <tok val="ᾠζ[ύυ]ρ[ʼ'’ὰ]?"/>
         <lm>
            <l>ὀϊζυρός</l>
            <m cert="0.125">a - - p - - - n n</m>
            <m cert="0.125">a - - p - - - n v</m>
            <m cert="0.125">a - - p - - - n a</m>
            <m cert="0.125">a - - d - - - f n</m>
            <m cert="0.125">a - - d - - - f v</m>
            <m cert="0.125">a - - d - - - f a</m>
            <m cert="0.125">a - - s - - - f n</m>
            <m cert="0.125">a - - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--ᾠζύρʼ ᾠζυρέ ᾠζυρὲ-->
         <tok val="ᾠζ[υύ]ρ[ὲʼ'’έ]"/>
         <lm>
            <l>ὀϊζυρός</l>
            <m>a - - s - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠζύρ[ʼ'’]?"/>

            <l>ὀϊζυρός</l>
            <m cert="0.5">a - - p - - - f n</m>
            <m cert="0.5">a - - p - - - f v</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὤζωσ[ίι]ν"/>
         <lm>
            <l>ὤζω</l>
            <m>v - 3 p p s a</m>
         </lm>
      </ana>
   </body>

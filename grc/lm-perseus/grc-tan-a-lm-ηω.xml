   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>Ἠῶ</tok-starts-with>
      <tok-starts-with>ἠῶ</tok-starts-with>
      <tok-starts-with>ἠῴ</tok-starts-with>
      <tok-starts-with>ἠώ</tok-starts-with>

      </ana>
      <ana>
         <tok val="Ἠώνης"/>
         <lm>
            <l>Ἠώνη</l>
            <m>n e - s - - - f g</m>
         </lm>
      </ana>
      <ana><!--ἠῴοις ἠῴοισιν-->
         <tok val="ἠῴοι[σ]?[ίι]?[νς]"/>

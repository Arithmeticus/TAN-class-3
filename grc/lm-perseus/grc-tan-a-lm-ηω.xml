      <ana><!--ἠώς ἠὼς Ἠώς Ἠὼς-->
         <tok val="[ἠἨ]?(ὼ|ἠώ|Ἠώ)ς"/>
         <lm>
            <l>ἠώς</l>
            <m>n - - s - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ἠῷο[ίι]"/>
         <lm>
            <l>ἠῷος</l>
            <m cert="0.5">a - - p - - - m n</m>
            <m cert="0.5">a - - p - - - m v</m>
         </lm>
      </ana>
   </body>

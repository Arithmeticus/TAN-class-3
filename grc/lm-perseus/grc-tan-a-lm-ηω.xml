      <tok-starts-with>Ἠώ</tok-starts-with>
      <tok-starts-with>ἠῷ</tok-starts-with>
      <tok-starts-with>ᾐώ</tok-starts-with>
      <tok-starts-with>ᾐω</tok-starts-with>
      <tok-starts-with>ἠὼ</tok-starts-with>
      <tok-starts-with>Ἠὼ</tok-starts-with>
      <tok-starts-with>Ἠῷ</tok-starts-with>
      <ana><!--Ἠῶ ἠῶ-->
         <tok val="[ἠἨ]ῶ"/>
         <lm>
            <l>ἠώς</l>
            <m cert="0.333333333333333333">n - - d - - - f n</m>
            <m cert="0.333333333333333333">n - - d - - - f v</m>
            <m cert="0.333333333333333333">n - - d - - - f a</m>
         </lm>
      </ana>
      <ana><!--Ἠῶ ἠῶ ἠὼ-->
         <tok val="[ἠἨ]?(ῶ|ἠὼ)"/>
         <lm>
            <l>ἠώς</l>
            <m>n - - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἠῶ"/>
         <lm>
            <l>ἀάω</l>
            <m>v - 2 s i i e</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἠῴην"/>
         <lm>
            <l>ἠῷος</l>
            <m>a - - s - - - f a</m>
         </lm>
      </ana>
      <ana><!--ἠῶθεν ἠῶθέν-->
         <tok val="ἠῶθ[έεὲ]ν"/>
         <lm cert="0.5">
            <l>ἠώς</l>
            <m>d</m>
         </lm>
         <lm cert="0.5">
            <l>ἠῶθεν</l>
            <m>d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἠῶθ[ίι]"/>

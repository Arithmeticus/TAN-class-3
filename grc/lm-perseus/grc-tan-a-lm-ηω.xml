            <l>ἠῷος</l>
            <m cert="0.5">a - - p - - - m d</m>
            <m cert="0.5">a - - p - - - n d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἠῷ[όο]ν"/>
         <lm>
            <l>ἠῷος</l>
            <m cert="0.25">a - - s - - - m a</m>
            <m cert="0.25">a - - s - - - n n</m>
            <m cert="0.25">a - - s - - - n v</m>
            <m cert="0.25">a - - s - - - n a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾐώρει"/>
         <lm>
            <l>αἰωρέω</l>
            <m>v - 3 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾐωρεῖτ[όο]"/>
         <lm>
            <l>αἰωρέω</l>
            <m>v - 3 s i i e</m>

<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusκατα">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to κατα</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CE%BA%CE%B1%CF%84%CE%B1.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>κατα</tok-starts-with>
      <tok-starts-with>ΚΑΤΑ</tok-starts-with>
      <tok-starts-with>Κατὰ</tok-starts-with>
      <tok-starts-with>κἆτα</tok-starts-with>
      <tok-starts-with>κάτα</tok-starts-with>
      <tok-starts-with>κᾆτα</tok-starts-with>
      <tok-starts-with>κᾆτά</tok-starts-with>
      <tok-starts-with>κατά</tok-starts-with>
      <tok-starts-with>κατὰ</tok-starts-with>
      <ana><!--κατα ΚΑΤΑ Κατὰ κάτα κατά κατὰ-->
         <tok val="[κ]?(άτ)?(ατ)?(ὰ|α|ά|Κατὰ|ΚΑΤΑ)"/>
         <lm>
            <l>κατά</l>
            <m>r</m>
         </lm>
      </ana>
      <ana>
         <tok val="κἆτ[άα]"/>
         <lm cert="0.5">
            <l>ἄτη</l>
            <m cert="0.2">n - - d - - - f n</m>
            <m cert="0.2">n - - d - - - f v</m>
            <m cert="0.2">n - - d - - - f a</m>
            <m cert="0.2">n - - s - - - f n</m>
            <m cert="0.2">n - - s - - - f v</m>
         </lm>
         <lm cert="0.5">
            <l>ἄατος</l>
            <m cert="0.333333333333333333">a - - p - - - n n</m>
            <m cert="0.333333333333333333">a - - p - - - n v</m>
            <m cert="0.333333333333333333">a - - p - - - n a</m>
         </lm>
      </ana>
      <ana><!--κᾆτα κᾆτά-->
         <tok val="κᾆτ[άαὰ]"/>
         <lm>
            <l>εἶτα</l>
            <m>d</m>
         </lm>
      </ana>
      <ana><!--κατὰ κατά-->
         <tok val="κατ[ὰά]"/>
         <lm cert="0.5">
            <l>κατά</l>
            <m>d</m>
         </lm>
         <lm cert="0.5">
            <l>καθά</l>
            <m>d r</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

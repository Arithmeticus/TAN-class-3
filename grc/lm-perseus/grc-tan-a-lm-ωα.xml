<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusωα">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ωα</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%89%CE%B1.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ᾠά</tok-starts-with>
      <tok-starts-with>ᾠὰ</tok-starts-with>
      <tok-starts-with>ᾠα</tok-starts-with>
      <tok-starts-with>Ὠα</tok-starts-with>
      <tok-starts-with>ὤα</tok-starts-with>
      <tok-starts-with>ΩΑ</tok-starts-with>
      <tok-starts-with>ὠὰ</tok-starts-with>
      <tok-starts-with>Ὤα</tok-starts-with>
      <ana><!--ᾠά ᾠὰ ὠὰ-->
         <tok val="[ᾠ]?(ὰ|ά|ὠὰ)"/>
         <lm>
            <l>ᾠόν</l>
            <m cert="0.333333333333333333">n - - p - - - n n</m>
            <m cert="0.333333333333333333">n - - p - - - n v</m>
            <m cert="0.333333333333333333">n - - p - - - n a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠακοστρόφουν"/>
         <lm>
            <l>οἰακοστροφέω</l>
            <m cert="0.5">v - 3 p i i a</m>
            <m cert="0.5">v - 1 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὠαριωνείαν"/>
         <lm>
            <l>Ὠαριώνειος</l>
            <m>a e - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὤας"/>
         <lm>
            <l>ἀάω</l>
            <m>v - 2 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ΩΑ"/>
         <lm>
            <l>ΑΒΓ</l>
            <m>y</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὠαρίων[όο]ς"/>
         <lm>
            <l>Ὠρίων</l>
            <m>n e - s - - - m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὠαρίων"/>
         <lm>
            <l>Ὠρίων</l>
            <m>n e - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὤαν[όο]ν"/>
         <lm>
            <l>Ὤανος</l>
            <m>n e - s - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὠαρίων[άα]"/>
         <lm>
            <l>Ὠρίων</l>
            <m>n e - s - - - m a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

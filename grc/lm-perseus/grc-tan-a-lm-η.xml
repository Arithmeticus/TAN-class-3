<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusη">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to η</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CE%B7.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>Ἢ</tok-starts-with>
      <tok-starts-with>Ἡ</tok-starts-with>
      <tok-starts-with>Ἥ</tok-starts-with>
      <tok-starts-with>Ἣ</tok-starts-with>
      <tok-starts-with>ἤ</tok-starts-with>
      <tok-starts-with>ἢ</tok-starts-with>
      <tok-starts-with>ἧ</tok-starts-with>
      <tok-starts-with>ᾗ</tok-starts-with>
      <tok-starts-with>ἡ</tok-starts-with>
      <tok-starts-with>ἥ</tok-starts-with>
      <tok-starts-with>ἣ</tok-starts-with>
      <tok-starts-with>ἦ</tok-starts-with>
      <tok-starts-with>ᾖ</tok-starts-with>
      <tok-starts-with>Ἦ</tok-starts-with>
      <tok-starts-with>Η</tok-starts-with>
      <tok-starts-with>ᾟ</tok-starts-with>
      <ana><!--Ἢ ἤ ἢ ἦ-->
         <tok val="[ἢἤἪἦ]"/>
         <lm>
            <l>ἤ</l>
            <m>c</m>
         </lm>
      </ana>
      <ana><!--Ἢ ἤ ἢ-->
         <tok val="[ἢἤἪ]"/>
         <lm>
            <l>ἤ2</l>
            <m>i</m>
         </lm>
      </ana>
      <ana><!--Ἡ Ἥ Ἣ ἧ ἡ ἥ ἣ-->
         <tok val="[ἫἣἭἩἧἡἥ]"/>
         <lm>
            <l>ὁ</l>
            <m>p a - s - - - f n</m>
         </lm>
      </ana>
      <ana><!--Ἥ Ἣ ἥ ἣ-->
         <tok val="[ἫἣἭἥ]"/>
         <lm>
            <l>ὅς</l>
            <m>p r - s - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἧ"/>
         <lm>
            <l>ἧ</l>
            <m>d</m>
         </lm>
      </ana>
      <ana><!--ᾗ ᾟ-->
         <tok val="[ᾟᾗ]"/>
         <lm>
            <l>ὅς</l>
            <m>d r</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾗ"/>
         <lm cert="0.333333333333333333">
            <l>ἵημι</l>
            <m cert="0.5">v - 2 s a s m</m>
            <m cert="0.5">v - 3 s a s a</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>ὅς</l>
            <m>p r - s - - - f d</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>ὅς2</l>
            <m>p s - s - - - f d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἦ"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 1 s i i a</m>
         </lm>
      </ana>
      <ana><!--ἦ Ἦ-->
         <tok val="[Ἦἦ]"/>
         <lm cert="0.5">
            <l>ἦ</l>
            <m>g</m>
         </lm>
         <lm cert="0.5">
            <l>ἠμί</l>
            <m>v - 3 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾖ"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 3 s p s a</m>
         </lm>
      </ana>
      <ana><!--ἢ ἤ-->
         <tok val="[ἢἤ]"/>
         <lm>
            <l>τίη</l>
            <m>d</m>
         </lm>
      </ana>
      <ana>
         <tok val="Η"/>
         <lm>
            <l>ΑΒΓ</l>
            <m>y</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ωυ</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%89%CF%85.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ὡυ</tok-starts-with>
      <tok-starts-with>ωὐ</tok-starts-with>
      <tok-starts-with>ωὑ</tok-starts-with>
      <tok-starts-with>Ωὑ</tok-starts-with>
      <tok-starts-with>ὠυ</tok-starts-with>
      <tok-starts-with>ωὖ</tok-starts-with>
      <ana><!--ὡυτοί ὡυτοὶ-->
         <tok val="ὡυτο[ὶί]"/>
         <lm>
            <l>αὐτός</l>
            <m cert="0.5">p p - p - - - m n</m>
            <m cert="0.5">p p - p - - - m v</m>
         </lm>
      </ana>
      <ana><!--ωὐτὸς ωὑτὸς ωὑτός Ωὑτὸς ωὐτός-->
         <tok val="[ω]?[ὑὐ]?[τ]?[ὸό]?(ς|Ωὑτὸς|ωὐτός)"/>
         <lm>
            <l>αὐτός</l>
            <m>a - - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὡυτὸς"/>
         <lm>
            <l>αὐτός</l>
            <m>p p - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὡυτ[ὴή]"/>
         <lm>
            <l>αὐτός</l>
            <m cert="0.5">a - - s - - - f n</m>
            <m cert="0.5">a - - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--ωὐτά ωὐτὰ-->
         <tok val="ωὐτ[ὰά]"/>
         <lm>
            <l>αὐτός</l>
            <m cert="0.166666666666666667">p - - d - - - f a</m>
            <m cert="0.166666666666666667">p - - d - - - f n</m>
            <m cert="0.166666666666666667">p - - d - - - f v</m>
            <m cert="0.166666666666666667">p - - p - - - n a</m>
            <m cert="0.166666666666666667">p - - p - - - n n</m>
            <m cert="0.166666666666666667">p - - p - - - n v</m>
         </lm>
      </ana>
      <ana><!--ωὐτά ωὐτὰ ωὐτή ωὐτὴ-->
         <tok val="ωὐτ[ὰὴάή]"/>
         <lm>
            <l>αὐτός</l>
            <m cert="0.5">p - - s - - - f n</m>
            <m cert="0.5">p - - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--ὡυτέων ωὐτῶν-->
         <tok val="(ὡυτέ)?ω(ὐτῶ)?ν"/>
         <lm>
            <l>αὐτός</l>
            <m cert="0.5">p - - p - - - f g</m>
            <m cert="0.5">p - - p - - - m g</m>
         </lm>
      </ana>
      <ana><!--ὠυτή ὠυτὴ-->
         <tok val="ὠυτ[ὴή]"/>
         <lm>
            <l>ἀυτή</l>
            <m cert="0.5">n - - s - - - f n</m>
            <m cert="0.5">n - - s - - - f v</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὠυτὴν"/>
         <lm>
            <l>ἀυτή</l>
            <m>n - - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ωὐτῶν"/>
         <lm>
            <l>αὐτός</l>
            <m>p - - p - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="ωὖριπίδη"/>
         <lm>
            <l>Εὐριπίδης</l>
            <m>n e - s - - - m v</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

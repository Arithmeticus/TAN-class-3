<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusω">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ω</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%89.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ὤ</tok-starts-with>
      <tok-starts-with>ὢ</tok-starts-with>
      <tok-starts-with>ᾦ</tok-starts-with>
      <tok-starts-with>ὥ</tok-starts-with>
      <tok-starts-with>ὣ</tok-starts-with>
      <tok-starts-with>ᾧ</tok-starts-with>
      <tok-starts-with>ὧ</tok-starts-with>
      <tok-starts-with>ὦ</tok-starts-with>
      <tok-starts-with>Ὦ</tok-starts-with>
      <tok-starts-with>Ω</tok-starts-with>
      <tok-starts-with>Ὢ</tok-starts-with>
      <tok-starts-with>ᾯ</tok-starts-with>
      <ana><!--ὤ ὢ ὦ Ὦ Ὢ-->
         <tok val="[ὢὤὦὮὪ]"/>
         <lm>
            <l>ὦ</l>
            <m>i</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾦ"/>
         <lm>
            <l>ὄϊς</l>
            <m>n - - s - - - c v</m>
         </lm>
      </ana>
      <ana><!--ὥ ὣ-->
         <tok val="[ὣὥ]"/>
         <lm>
            <l>ὅς</l>
            <m cert="0.333333333333333333">p r - d - - - m a</m>
            <m cert="0.333333333333333333">p r - d - - - n a</m>
            <m cert="0.333333333333333333">p r - d - - - m n</m>
         </lm>
      </ana>
      <ana><!--ᾧ ᾯ-->
         <tok val="[ᾯᾧ]"/>
         <lm>
            <l>ὅς</l>
            <m>p r - s - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾧ"/>
         <lm cert="0.5">
            <l>ὅς</l>
            <m>p r - s - - - n d</m>
         </lm>
         <lm cert="0.5">
            <l>ὅς2</l>
            <m cert="0.5">p s - s - - - m d</m>
            <m cert="0.5">p s - s - - - n d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὧ"/>
         <lm cert="0.5">
            <l>ἵημι</l>
            <m>v - 1 s a s a</m>
         </lm>
         <lm cert="0.5">
            <l>ὅς</l>
            <m cert="0.5">p r - s - - - m g</m>
            <m cert="0.5">p r - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὦ"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 1 s p s a</m>
         </lm>
      </ana>
      <ana><!--ὦ Ὦ-->
         <tok val="[Ὦὦ]"/>
         <lm>
            <l>ὦ</l>
            <m>e</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ὦ"/>
         <lm>
            <l>Ω</l>
            <m>n - - s - - - n n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Ω"/>
         <lm>
            <l>ΑΒΓ</l>
            <m>y</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὣ"/>
         <lm cert="0.5">
            <l>ὅς2</l>
            <m>p s - d - - - m a</m>
         </lm>
         <lm cert="0.5">
            <l>ὅς</l>
            <m>p r - d - - - f n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

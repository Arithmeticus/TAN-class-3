<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusηψ">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ηψ</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CE%B7%CF%88.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ἧψ</tok-starts-with>
      <tok-starts-with>ἥψ</tok-starts-with>
      <tok-starts-with>ἡψ</tok-starts-with>
      <tok-starts-with>Ἥψ</tok-starts-with>
      <ana>
         <tok val="ἧψ[ὰά]"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 1 s a i a</m>
         </lm>
      </ana>
      <ana><!--ἥψαθʼ ἥψατʼ ἥψατο Ἥψατο Ἥψατό-->
         <tok val="[ἭἥἫ]ψα(τό|θ|θʼ|θ'|θ’|τ|τʼ|το|τ'|τ’|τὸ)"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 3 s a i m</m>
         </lm>
      </ana>
      <ana><!--ἥψαθʼ ἥψατʼ-->
         <tok val="ἥψα[θ]?(ʼ|'|’|τ|τʼ|τ'|τ’)?"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 2 p a i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἡψάμεθ[άα]"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 1 p a i m</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἡψάμην"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 1 s a i m</m>
         </lm>
      </ana>
      <ana><!--ἧψαν ἧψάν-->
         <tok val="ἧψ[άαὰ]ν"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 3 p a i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἥψαντ[όο]"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 3 p a i m</m>
         </lm>
      </ana>
      <ana><!--ἥψαο ἥψω-->
         <tok val="ἥψ(ω|αο)"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 2 s a i m</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἧψ[άα]ς"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 2 s a i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἥψασθ[έε]"/>
         <lm>
            <l>ἅπτω</l>
            <m>v - 2 p a i m</m>
         </lm>
      </ana>
      <ana><!--ἧψε ἧψεν-->
         <tok val="ἧψ[ε]?(ν|έν|έ)?"/>
         <lm cert="0.5">
            <l>ἅπτω</l>
            <m>v - 3 s a i a</m>
         </lm>
         <lm cert="0.5">
            <l>ἕψω</l>
            <m>v - 3 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="[ἣἥ]ψετ[ὸό]"/>
         <lm>
            <l>ἕψω</l>
            <m>v - 3 s i i e</m>
         </lm>
      </ana>
      <ana><!--ἥψησε ἥψησεν-->
         <tok val="ἥψησ[ε]?(ν|έν|έ)?"/>
         <lm>
            <l>ἕψω</l>
            <m>v - 3 s a i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἥψομ[έε]ν"/>
         <lm>
            <l>ἕψω</l>
            <m>v - 1 p i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἡψῆσθα[ίι]"/>
         <lm>
            <l>ἕψω</l>
            <m>v - - - r n e</m>
         </lm>
      </ana>
      <ana>
         <tok val="[ἣἥ]ψησ[ὰά]ν"/>
         <lm>
            <l>ἕψω</l>
            <m>v - 3 p a i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

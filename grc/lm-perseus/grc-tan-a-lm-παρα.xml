   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>παρά</tok-starts-with>
      <tok-starts-with>παρὰ</tok-starts-with>
      <tok-starts-with>πάρα</tok-starts-with>
      <tok-starts-with>Παρὰ</tok-starts-with>
      <ana><!--παρά παρὰ πάρα Παρὰ-->
         <tok val="(π|Π|πάρ)αρ[ὰά]?"/>
         <lm>
            <l>παρά</l>
            <m>r</m>
         </lm>
      </ana>
      <ana>
         <tok val="παρὰ"/>
         <lm>
            <l>πηρός</l>
            <m cert="0.125">a - - p - - - n n</m>
            <m cert="0.125">a - - p - - - n v</m>
            <m cert="0.125">a - - p - - - n a</m>
            <m cert="0.125">a - - d - - - f n</m>
            <m cert="0.125">a - - d - - - f v</m>
            <m cert="0.125">a - - d - - - f a</m>
            <m cert="0.125">a - - s - - - f n</m>
            <m cert="0.125">a - - s - - - f v</m>
         </lm>
      </ana>
      <ana>
         <tok val="πάρα"/>
         <lm cert="0.5">
            <l>πῆρος</l>
            <m cert="0.5">n - - p - - - n n</m>
            <m cert="0.5">n - - p - - - n a</m>
         </lm>
         <lm cert="0.5">
            <l>πάρειμι</l>
            <m>v - 3 s p i a</m>

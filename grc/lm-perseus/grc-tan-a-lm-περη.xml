   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>πέρη</tok-starts-with>
      <tok-starts-with>περῆ</tok-starts-with>
      <tok-starts-with>περή</tok-starts-with>
      <tok-starts-with>περη</tok-starts-with>
      <tok-starts-with>περῇ</tok-starts-with>
      <ana><!--πέρηθε πέρην-->
         <tok val="πέρη[θ]?[έεν]"/>
         <lm>
            <l>πέραν</l>
            <m>d</m>
         </lm>
      </ana>
      <ana>
         <tok val="περῆν[ʼ'’]?"/>
         <lm>
            <l>περαίνω</l>
            <m>v - - - a n a</m>
         </lm>
      </ana>
      <ana>
         <tok val="περήσαμ[έε]ν"/>
         <lm>
            <l>περάω</l>
            <m>v - 1 p a i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="περήσαντ[έε]ς"/>
         <lm>
            <l>περάω</l>
            <m cert="0.5">v - - p a p a m n</m>
            <m cert="0.5">v - - p a p a m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="περήσας"/>

<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusεη">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to εη</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CE%B5%CE%B7.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ἑῇ</tok-starts-with>
      <tok-starts-with>ἑή</tok-starts-with>
      <tok-starts-with>ἐή</tok-starts-with>
      <tok-starts-with>ἐὴ</tok-starts-with>
      <tok-starts-with>ἔῃ</tok-starts-with>
      <tok-starts-with>ἑῆ</tok-starts-with>
      <tok-starts-with>ἕη</tok-starts-with>
      <tok-starts-with>ἑὴ</tok-starts-with>
      <tok-starts-with>ἔη</tok-starts-with>
      <tok-starts-with>ΕΗ</tok-starts-with>
      <tok-starts-with>ἐῆ</tok-starts-with>
      <ana><!--ἑῇ ἑῆι-->
         <tok val="ἑ[ῆ]?[ίιῇ]"/>
         <lm cert="0.5">
            <l>ἑός</l>
            <m>p s - s - - - f d</m>
         </lm>
         <lm cert="0.5">
            <l>ἵημι</l>
            <m>v - 2 s p s e</m>
         </lm>
      </ana>
      <ana><!--ἑῇ ἑῆι ἑῇσι ἑῇσιν-->
         <tok val="ἑ(ῇσ)?[ι]?(ν|ίν|ί|ῆί|ῆι)?"/>
         <lm>
            <l>ἵημι</l>
            <m>v - 3 s p s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἑ[ὴή]"/>
         <lm>
            <l>ἑός</l>
            <m cert="0.5">p s - s - - - f n</m>
            <m cert="0.5">p s - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--ἐή ἐὴ-->
         <tok val="ἐ[ὴή]"/>
         <lm>
            <l>ἐέ</l>
            <m>i</m>
         </lm>
      </ana>
      <ana><!--ἔῃ ἔῃσι ἔῃσιν-->
         <tok val="ἔῃσι[ν]?"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 3 s p s a</m>
         </lm>
      </ana>
      <ana><!--ἕηκε ἕηκεν-->
         <tok val="ἕηκ[ε]?(ν|έν|έ)?"/>
         <lm>
            <l>ἵημι</l>
            <m>v - 3 s a i a</m>
         </lm>
      </ana>
      <ana><!--ἑήν ἑὴν-->
         <tok val="ἑ[ὴή]ν"/>
         <lm>
            <l>ἑός</l>
            <m>p s - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἔην"/>
         <lm>
            <l>εἰμί</l>
            <m cert="0.5">v c 1 s i i a</m>
            <m cert="0.5">v c 3 s i i a</m>
         </lm>
      </ana>
      <ana><!--ἑήνδανε ἑήνδανεν-->
         <tok val="ἑήνδαν[ε]?(ν|έν|έ)?"/>
         <lm>
            <l>ἁνδάνω</l>
            <m>v - 3 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἑῆ[όο]ς"/>
         <lm>
            <l>ἐΰς</l>
            <m cert="0.333333333333333333">a - - s - - - m g</m>
            <m cert="0.333333333333333333">a - - s - - - f g</m>
            <m cert="0.333333333333333333">a - - s - - - n g</m>
         </lm>
      </ana>
      <ana><!--ἑῇς ἑῇσι ἑῇσιν-->
         <tok val="ἑῇ[σ]?[ι]?(ν|ίν|ί|ς)?"/>
         <lm>
            <l>ἑός</l>
            <m>p s - p - - - f d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἑῇς"/>
         <lm>
            <l>ἵημι</l>
            <m>v - 2 s p s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἑῆς"/>
         <lm>
            <l>ἑός</l>
            <m>p s - s - - - f g</m>
         </lm>
      </ana>
      <ana><!--ἑῆς ἕης-->
         <tok val="(ἕη|ἑῆ)ς"/>
         <lm>
            <l>ὅς</l>
            <m>p r - s - - - f g</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἕης"/>
         <lm>
            <l>ἵημι</l>
            <m>v - 2 s p i a</m>
         </lm>
      </ana>
      <ana><!--ἔῃς ἔησθα-->
         <tok val="ἔ(ησθ)?(ά|α|ῃς)"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 2 s p s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἔησθ[άα]"/>
         <lm>
            <l>εἰμί</l>
            <m>v c 2 s i i a</m>
         </lm>
      </ana>
      <ana><!--ΕΗ ΕΗΖ ΕΗΒ ΕΗΓΖ ΕΗΓΘ ΕΗΔ ΕΗΓ ΕΗΓΔ-->
         <tok val="ΕΗ[Γ]?(Ζ|Γ|ΓΘ|ΓΔ|Δ|Β)?"/>
         <lm>
            <l>ΑΒΓ</l>
            <m>y</m>
         </lm>
      </ana>
      <ana>
         <tok val="ἐῆν"/>
         <lm>
            <l>ἐάω</l>
            <m>v - - - p n a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

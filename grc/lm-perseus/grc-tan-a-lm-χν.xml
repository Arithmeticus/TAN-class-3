<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusχν">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to χν</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%87%CE%BD.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>χν</tok-starts-with>
      <ana>
         <tok val="χναύειν"/>
         <lm>
            <l>χναύω</l>
            <m>v - - - p n a</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνοάζων"/>
         <lm>
            <l>χνοάζω</l>
            <m>v - - s p p a m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνόαι"/>
         <lm>
            <l>χνόη</l>
            <m cert="0.333333333333333333">n - - p - - - f n</m>
            <m cert="0.333333333333333333">n - - p - - - f v</m>
            <m cert="0.333333333333333333">n - - s - - - f d</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνόας"/>
         <lm cert="0.5">
            <l>χνόη</l>
            <m cert="0.5">n - - p - - - f a</m>
            <m cert="0.5">n - - s - - - f g</m>
         </lm>
         <lm cert="0.5">
            <l>χνοάω</l>
            <m>v - 2 s i i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνόον"/>
         <lm>
            <l>χνόος</l>
            <m>n - - s - - - m a</m>
         </lm>
      </ana>
      <ana><!--χνόος χνοῦς-->
         <tok val="χν[ό]?ο[ῦ]?ς"/>
         <lm>
            <l>χνόος</l>
            <m>n - - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνοῦς"/>
         <lm>
            <l>χνόος</l>
            <m>n - - p - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνοῶδ[έε]ς"/>
         <lm>
            <l>χνοώδης</l>
            <m cert="0.2">a - - s - - - f v</m>
            <m cert="0.2">a - - s - - - m v</m>
            <m cert="0.2">a - - s - - - n a</m>
            <m cert="0.2">a - - s - - - n n</m>
            <m cert="0.2">a - - s - - - n v</m>
         </lm>
      </ana>
      <ana>
         <tok val="χνοώδης"/>
         <lm>
            <l>χνοώδης</l>
            <m cert="0.125">a - - p - - - f a</m>
            <m cert="0.125">a - - p - - - f n</m>
            <m cert="0.125">a - - p - - - f v</m>
            <m cert="0.125">a - - p - - - m a</m>
            <m cert="0.125">a - - p - - - m n</m>
            <m cert="0.125">a - - p - - - m v</m>
            <m cert="0.125">a - - s - - - f n</m>
            <m cert="0.125">a - - s - - - m n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

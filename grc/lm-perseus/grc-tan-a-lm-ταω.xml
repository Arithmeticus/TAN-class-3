      <tok-starts-with>ταώ</tok-starts-with>
      <ana>
         <tok val="Ταῲ"/>
         <lm>
            <l>Ταώς</l>
            <m>n e - s - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ταὼν"/>
         <lm>
            <l>ταώς</l>
            <m cert="0.5">n - - s - - - m a</m>
            <m cert="0.5">n - - s - - - m v</m>
         </lm>
      </ana>
      <ana><!--ταὼν ταὧς-->
         <tok val="τα(ὧς|ὼν)"/>
         <lm>
            <l>ταώς</l>
            <m>n - - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="τάων"/>
         <lm>
            <l>ὁ</l>
            <m>p a - p - - - f g</m>
         </lm>
      </ana>
      <ana><!--ταῶνας ταώς-->
         <tok val="τα(ῶν)?[άα]?(ς|ὼς|ώς)"/>

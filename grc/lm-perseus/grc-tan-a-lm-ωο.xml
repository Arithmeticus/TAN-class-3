<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:grc:perseusωο">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ωο</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/grc/lm-perseus/grc-tan-a-lm-%CF%89%CE%BF.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LSJ" which="Liddell-Scott-Jones 9+"/>
         <morphology xml:id="perseus-dik" which="Perseus Greek morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LSJ" morphology="perseus-dik">
      <for-lang>grc</for-lang>
      <tok-starts-with>ᾠό</tok-starts-with>
      <tok-starts-with>ὠὸ</tok-starts-with>
      <tok-starts-with>ᾠὸ</tok-starts-with>
      <tok-starts-with>ᾤο</tok-starts-with>
      <tok-starts-with>ὠό</tok-starts-with>
      <tok-starts-with>ᾠο</tok-starts-with>
      <tok-starts-with>ΩΟ</tok-starts-with>
      <tok-starts-with>ὠο</tok-starts-with>
      <ana><!--ᾠόμεθʼ ᾠόμεθα-->
         <tok val="ᾠόμεθ[ʼα'’ά]?"/>
         <lm>
            <l>οἴομαι</l>
            <m>v - 1 p i i m</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠόμην"/>
         <lm>
            <l>οἴομαι</l>
            <m>v - 1 s i i m</m>
         </lm>
      </ana>
      <ana><!--ὠὸν ᾠόν ᾠὸν-->
         <tok val="[ᾠὠ]?(ὸ|ᾠό)ν"/>
         <lm>
            <l>ᾠόν</l>
            <m cert="0.333333333333333333">n - - s - - - n n</m>
            <m cert="0.333333333333333333">n - - s - - - n v</m>
            <m cert="0.333333333333333333">n - - s - - - n a</m>
         </lm>
      </ana>
      <ana><!--ᾤονθʼ ᾤοντʼ ᾤοντο ᾤοντό-->
         <tok val="[ᾤᾢ]ον(τό|θ|θʼ|θ'|θ’|τ|τʼ|το|τ'|τ’|τὸ)"/>
         <lm>
            <l>οἴομαι</l>
            <m>v - 3 p i i m</m>
         </lm>
      </ana>
      <ana><!--ὠόπ ὠὸπ-->
         <tok val="ὠ[ὸό]π"/>
         <lm>
            <l>ὠόπ</l>
            <m>i</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠοτοκεῖ"/>
         <lm cert="0.5">
            <l>ᾠοτοκέω</l>
            <m cert="0.5">v - 2 s p i e</m>
            <m cert="0.5">v - 3 s p i a</m>
         </lm>
         <lm cert="0.5">
            <l>ᾠοτοκεύς</l>
            <m>n - - s - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠοῦ"/>
         <lm cert="0.333333333333333333">
            <l>οἰάω</l>
            <m>v - 2 s i i e</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>οἰόομαι</l>
            <m>v - 2 s i i m</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>ᾠόν</l>
            <m>n - - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾤου"/>
         <lm cert="0.5">
            <l>οἴομαι</l>
            <m>v - 2 s i i m</m>
         </lm>
         <lm cert="0.5">
            <l>ᾦον</l>
            <m>n - - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="ΩΟ"/>
         <lm>
            <l>ΑΒΓ</l>
            <m>y</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠοειδεῖς"/>
         <lm>
            <l>ᾠοειδής</l>
            <m cert="0.166666666666666667">a - - p - - - f a</m>
            <m cert="0.166666666666666667">a - - p - - - f n</m>
            <m cert="0.166666666666666667">a - - p - - - f v</m>
            <m cert="0.166666666666666667">a - - p - - - m a</m>
            <m cert="0.166666666666666667">a - - p - - - m n</m>
            <m cert="0.166666666666666667">a - - p - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="ὠοῖς"/>
         <lm>
            <l>ᾠόν</l>
            <m>n - - p - - - n d</m>
         </lm>
      </ana>
      <ana>
         <tok val="ᾠοτόκων"/>
         <lm>
            <l>ᾠότοκος</l>
            <m cert="0.333333333333333333">a - - p - - - f g</m>
            <m cert="0.333333333333333333">a - - p - - - m g</m>
            <m cert="0.333333333333333333">a - - p - - - n g</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

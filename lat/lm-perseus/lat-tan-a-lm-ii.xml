<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusii">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to ii</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-ii.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>II</tok-starts-with>
      <tok-starts-with>Ii</tok-starts-with>
      <tok-starts-with>ii</tok-starts-with>
      <tok-starts-with>íi</tok-starts-with>
      <tok-starts-with>ií</tok-starts-with>
      <ana>
         <tok val="II"/>
         <lm>
            <l>II</l>
            <m>m</m>
         </lm>
      </ana>
      <ana><!--II IIn Ii Iin ii iine iique-->
         <tok val="[I]?[I]?(n|in|i|iine|ii|iique)?"/>
         <lm cert="0.5">
            <l>eo</l>
            <m>v - 1 s r i a</m>
         </lm>
         <lm cert="0.5">
            <l>is</l>
            <m>p a - p - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="III"/>
         <lm>
            <l>III</l>
            <m>m</m>
         </lm>
      </ana>
      <ana><!--III IIIn IIi Iii-->
         <tok val="I[I]?[I]?(n|i|ii)?"/>
         <lm>
            <l>Iius</l>
            <m cert="0.333333333333333333">n e - p - - - m n</m>
            <m cert="0.333333333333333333">n e - s - - - m g</m>
            <m cert="0.333333333333333333">n e - p - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="IIII"/>
         <lm>
            <l>IIII</l>
            <m>m</m>
         </lm>
      </ana>
      <ana><!--IIIS IIIn-->
         <tok val="III[nS]"/>
         <lm>
            <l>Iius</l>
            <m cert="0.5">n e - p - - - m b</m>
            <m cert="0.5">n e - p - - - m d</m>
         </lm>
      </ana>
      <ana><!--IIS IIn Iin Iis Iisque iis iisne iisque iisue-->
         <tok val="[iI]?(is)?[q]?[un]?(e|IIn|IIS|Iin|Iis)?"/>
         <lm>
            <l>is</l>
            <m cert="0.333333333333333333">p a - p - - - m d</m>
            <m cert="0.333333333333333333">p a - p - - - f d</m>
            <m cert="0.333333333333333333">p a - p - - - n d</m>
         </lm>
      </ana>
      <ana><!--IIsdem Iisdem iisdem iisdemque-->
         <tok val="[I]?(i|I|ii)sdem(que)?"/>
         <lm>
            <l>idem</l>
            <m cert="0.333333333333333333">p a - p - - - m d</m>
            <m cert="0.333333333333333333">p a - p - - - f d</m>
            <m cert="0.333333333333333333">p a - p - - - n d</m>
         </lm>
      </ana>
      <ana><!--Ii Iin-->
         <tok val="Ii[n]?"/>
         <lm>
            <l>Ios</l>
            <m cert="0.333333333333333333">n e - p - - - f n</m>
            <m cert="0.333333333333333333">n e - p - - - f v</m>
            <m cert="0.333333333333333333">n e - s - - - f g</m>
         </lm>
      </ana>
      <ana><!--Iin Iis Iisque-->
         <tok val="Ii(s|n|sque)"/>
         <lm>
            <l>Ios</l>
            <m cert="0.5">n e - p - - - f d</m>
            <m cert="0.5">n e - p - - - f b</m>
         </lm>
      </ana>
      <ana><!--Iit iit iít-->
         <tok val="[iI]?i[í]?t"/>
         <lm>
            <l>eo</l>
            <m>v - 3 s r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Iius"/>
         <lm>
            <l>Iius</l>
            <m>n e - s - - - m n</m>
         </lm>
      </ana>
      <ana><!--iis Iis íis-->
         <tok val="[Iií]is"/>
         <lm>
            <l>is</l>
            <m>p a - p - - - m b</m>
         </lm>
      </ana>
      <ana><!--iis Iis-->
         <tok val="[Ii]is"/>
         <lm>
            <l>is</l>
            <m cert="0.5">p a - p - - - f b</m>
            <m cert="0.5">p a - p - - - n b</m>
         </lm>
      </ana>
      <ana>
         <tok val="iisse"/>
         <lm>
            <l>eo</l>
            <m>v - - - r n a</m>
         </lm>
      </ana>
      <ana>
         <tok val="iisses"/>
         <lm>
            <l>eo</l>
            <m>v - 2 s l s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="iisset"/>
         <lm>
            <l>eo</l>
            <m>v - 3 s l s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="iisti"/>
         <lm>
            <l>eo</l>
            <m>v - 2 s r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="iidem"/>
         <lm>
            <l>idem</l>
            <m>p a - p - - - m n</m>
         </lm>
      </ana>
      <ana><!--Iidem iidemque-->
         <tok val="[iI]idem(que)?"/>
         <lm>
            <l>idem</l>
            <m>p - - p - - - m n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

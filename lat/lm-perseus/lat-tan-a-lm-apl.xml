<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusapl">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to apl</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-apl.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>APL</tok-starts-with>
      <tok-starts-with>Apl</tok-starts-with>
      <tok-starts-with>apl</tok-starts-with>
      <ana><!--APLVSTRE Aplustre aplustre-->
         <tok val="[aA]?(plustre|APLVSTRE)"/>
         <lm>
            <l>aplustre</l>
            <m cert="0.333333333333333333">n - - s - - - n v</m>
            <m cert="0.333333333333333333">n - - s - - - n a</m>
            <m cert="0.333333333333333333">n - - s - - - n n</m>
         </lm>
      </ana>
      <ana><!--APLVSTRE Aplustre aplustre aplustri-->
         <tok val="[aA]?(plustr)?(e|i|APLVSTRE)"/>
         <lm>
            <l>aplustre</l>
            <m>n - - s - - - n b</m>
         </lm>
      </ana>
      <ana><!--Aplanes aplanes-->
         <tok val="[aA]planes"/>
         <lm>
            <l>aplanes</l>
            <m>v - 2 s p s a</m>
         </lm>
      </ana>
      <ana><!--Aple aple-->
         <tok val="[aA]ple"/>
         <lm>
            <l>apleo</l>
            <m>v - 2 s p m a</m>
         </lm>
      </ana>
      <ana><!--Apluda apluda-->
         <tok val="[aA]pluda"/>
         <lm>
            <l>apluda</l>
            <m cert="0.333333333333333333">n - - s - - - f b</m>
            <m cert="0.333333333333333333">n - - s - - - f n</m>
            <m cert="0.333333333333333333">n - - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--Aplysiae aplysiae-->
         <tok val="[aA]plysiae"/>
         <lm>
            <l>aplysiae</l>
            <m cert="0.5">n - - p - - - f v</m>
            <m cert="0.5">n - - p - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplicetur"/>
         <lm>
            <l>aplico</l>
            <m>v - 3 s p s p</m>
         </lm>
      </ana>
      <ana>
         <tok val="apludam"/>
         <lm>
            <l>apluda</l>
            <m>n - - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplustria"/>
         <lm>
            <l>aplustre</l>
            <m cert="0.333333333333333333">n - - p - - - n a</m>
            <m cert="0.333333333333333333">n - - p - - - n n</m>
            <m cert="0.333333333333333333">n - - p - - - n v</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplustribus"/>
         <lm>
            <l>aplustre</l>
            <m cert="0.5">n - - p - - - n d</m>
            <m cert="0.5">n - - p - - - n b</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplustris"/>
         <lm>
            <l>aplustre</l>
            <m>n - - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplustrium"/>
         <lm>
            <l>aplustre</l>
            <m>n - - p - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="aplustri"/>
         <lm>
            <l>aplustre</l>
            <m>n - - s - - - n d</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

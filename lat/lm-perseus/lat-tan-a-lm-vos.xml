   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Vos</tok-starts-with>
      <tok-starts-with>vos</tok-starts-with>
      <tok-starts-with>vós</tok-starts-with>
      <tok-starts-with>Vós</tok-starts-with>
      <ana><!--Vos Vosmet Vosne Vosque vos vosmet vosne vosque vost vós Vós-->
         <tok val="[vV]?(o|vó|Vó)s(t|met|que|ne)?"/>
         <lm>
            <l>tu</l>
            <m>p p - p - - - - a</m>
         </lm>
      </ana>
      <ana><!--Vos Vosmet Vosne Vosque vos vosmet vosne vosque vost-->
         <tok val="[vV]os(t|met|que|ne)?"/>
         <lm>
            <l>tu</l>
            <m>p p - p - - - - n</m>
         </lm>
      </ana>
      <ana><!--Vos Vosne Vosque vos vosne vosque vost-->
         <tok val="[vV]os(t|que|ne)?"/>
         <lm>
            <l>tu</l>
            <m>p p - p - - - - v</m>
         </lm>
      </ana>
      <ana><!--Vostrum vostrorum vostrum vostrumque vostrumst-->
         <tok val="[vV]ostr(or)?um(st|que)?"/>
         <lm>
            <l>vester</l>
            <m cert="0.5">p s - p - - - n g</m>
            <m cert="0.5">p s - p - - - m g</m>
         </lm>
      </ana>
      <ana><!--Vostrum vostrum vostrumque vostrumst-->
         <tok val="[vV]ostrum(st|que)?"/>

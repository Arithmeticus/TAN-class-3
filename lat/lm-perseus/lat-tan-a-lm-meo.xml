<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusmeo">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to meo</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-meo.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>MEO</tok-starts-with>
      <tok-starts-with>Meo</tok-starts-with>
      <tok-starts-with>meo</tok-starts-with>
      <tok-starts-with>meó</tok-starts-with>
      <tok-starts-with>méo</tok-starts-with>
      <ana><!--MEO Meo Meon meo meon meone meoque meost-->
         <tok val="[mM]?(eo)?[n]?(e|st|que|MEO)?"/>
         <lm cert="0.5">
            <l>meo</l>
            <m>v - 1 s p i a</m>
         </lm>
         <lm cert="0.5">
            <l>meum</l>
            <m cert="0.5">n - - s - - - n d</m>
            <m cert="0.5">n - - s - - - n b</m>
         </lm>
      </ana>
      <ana><!--MEO Meo Meon meo meon meone meoque meost meopte meó méo-->
         <tok val="[mM]?[e]?[o]?[n]?(e|st|que|pte|ó|méo|MEO)?"/>
         <lm>
            <l>meus</l>
            <m cert="0.25">p s - s - - - m b</m>
            <m cert="0.25">p s - s - - - n d</m>
            <m cert="0.25">p s - s - - - n b</m>
            <m cert="0.25">p s - s - - - m d</m>
         </lm>
      </ana>
      <ana><!--MEORVM Meorum meorum meorumque-->
         <tok val="[mM]?(eorum)?(que|MEORVM)?"/>
         <lm cert="0.333333333333333333">
            <l>mei</l>
            <m>n - - p - - - m g</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>meus</l>
            <m cert="0.5">p s - p - - - n g</m>
            <m cert="0.5">p s - p - - - m g</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>meum</l>
            <m>n - - p - - - n g</m>
         </lm>
      </ana>
      <ana><!--Meon Meos Meosne Meosque meon meos meosque meost-->
         <tok val="[mM]eo[s]?(t|que|ne|n|s)?"/>
         <lm cert="0.5">
            <l>meus</l>
            <m>p s - p - - - m a</m>
         </lm>
         <lm cert="0.5">
            <l>mei</l>
            <m>n - - p - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="meor"/>
         <lm>
            <l>meo</l>
            <m>v - 1 s p i p</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

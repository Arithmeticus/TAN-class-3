<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseussee">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to see</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-see.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>see</tok-starts-with>
      <ana>
         <tok val="seeleris"/>
         <lm>
            <l>seelino</l>
            <m>v - 2 s r s a</m>
         </lm>
      </ana>
      <ana><!--seen sees-->
         <tok val="see[sn]"/>
         <lm>
            <l>seedo</l>
            <m cert="0.5">v - 2 s p i a</m>
            <m cert="0.5">v - 2 s p m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="seere"/>
         <lm>
            <l>seeo</l>
            <m>v - 3 p r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="seeundi"/>
         <lm>
            <l>seeo</l>
            <m cert="0.2">v - - p p g p m v</m>
            <m cert="0.2">v - - s p d a n g</m>
            <m cert="0.2">v - - s p g p n g</m>
            <m cert="0.2">v - - p p g p m n</m>
            <m cert="0.2">v - - s p g p m g</m>
         </lm>
      </ana>
      <ana><!--seeundorum seeundum-->
         <tok val="seeund(or)?um"/>
         <lm>
            <l>seeo</l>
            <m cert="0.5">v - - p p g p n g</m>
            <m cert="0.5">v - - p p g p m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="seeundum"/>
         <lm>
            <l>seeo</l>
            <m cert="0.2">v - - s p g p n a</m>
            <m cert="0.2">v - - s p d a n a</m>
            <m cert="0.2">v - - s p g p m a</m>
            <m cert="0.2">v - - s p g p n v</m>
            <m cert="0.2">v - - s p g p n n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

      </ana>
      <ana>
         <tok val="CONFVNDO"/>
         <lm>
            <l>confundo</l>
            <m>v - 1 s p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Confvtatis"/>
         <lm>
            <l>confuto</l>
            <m cert="0.142857142857142857">v - 2 p p i a</m>
            <m cert="0.142857142857142857">v - - p r p p n d</m>
            <m cert="0.142857142857142857">v - - p r p p f d</m>
            <m cert="0.142857142857142857">v - - p r p p n b</m>

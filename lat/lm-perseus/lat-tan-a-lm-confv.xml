   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>CONFV</tok-starts-with>
      <tok-starts-with>Confv</tok-starts-with>
      <ana>
         <tok val="CONFVGIO"/>
         <lm cert="0.5">
            <l>confugio</l>
            <m>v - 1 s p i a</m>
         </lm>
         <lm cert="0.5">
            <l>confugium</l>
            <m cert="0.5">n - - s - - - n d</m>
            <m cert="0.5">n - - s - - - n b</m>

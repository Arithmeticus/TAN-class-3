<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseussupero">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to supero</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-supero.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Supero</tok-starts-with>
      <tok-starts-with>supero</tok-starts-with>
      <ana><!--Supero supero superoque-->
         <tok val="[sS]upero(que)?"/>
         <lm cert="0.5">
            <l>superus</l>
            <m cert="0.25">a - - s - - - n b</m>
            <m cert="0.25">a - - s - - - m d</m>
            <m cert="0.25">a - - s - - - n d</m>
            <m cert="0.25">a - - s - - - m b</m>
         </lm>
         <lm cert="0.5">
            <l>supero</l>
            <m>v - 1 s p i a</m>
         </lm>
      </ana>
      <ana><!--Superorum superorum-->
         <tok val="[sS]uperorum"/>
         <lm cert="0.333333333333333333">
            <l>superus</l>
            <m cert="0.5">a - - p - - - n g</m>
            <m cert="0.5">a - - p - - - m g</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>superi</l>
            <m>n - - p - - - m g</m>
         </lm>
         <lm cert="0.333333333333333333">
            <l>supera</l>
            <m>n - - p - - - n g</m>
         </lm>
      </ana>
      <ana><!--Superos superos superosne superosque-->
         <tok val="[sS]uperos(ne|que)?"/>
         <lm>
            <l>superus</l>
            <m>a - - p - - - m a</m>
         </lm>
      </ana>
      <ana><!--Superos superos-->
         <tok val="[sS]uperos"/>
         <lm>
            <l>superi</l>
            <m>n - - p - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="superobducto"/>
         <lm>
            <l>superobductus</l>
            <m cert="0.25">a - - s - - - n b</m>
            <m cert="0.25">a - - s - - - m d</m>
            <m cert="0.25">a - - s - - - n d</m>
            <m cert="0.25">a - - s - - - m b</m>
         </lm>
      </ana>
      <ana>
         <tok val="superoccupat"/>
         <lm>
            <l>superoccupo</l>
            <m>v - 3 s p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="superor"/>
         <lm>
            <l>supero</l>
            <m>v - 1 s p i p</m>
         </lm>
      </ana>
      <ana>
         <tok val="superordinat"/>
         <lm>
            <l>superordino</l>
            <m>v - 3 s p i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

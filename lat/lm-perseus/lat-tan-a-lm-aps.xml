<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusaps">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to aps</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-aps.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>APS</tok-starts-with>
      <tok-starts-with>APs</tok-starts-with>
      <tok-starts-with>Aps</tok-starts-with>
      <tok-starts-with>aps</tok-starts-with>
      <tok-starts-with>áps</tok-starts-with>
      <ana><!--APS APSn APs Aps aps apsque áps apsqué-->
         <tok val="(APS)?(n|apsqué|apsque|APs|Aps|aps|áps)?"/>
         <lm>
            <l>ab</l>
            <m>r</m>
         </lm>
      </ana>
      <ana><!--APSIS Apsin Apsis-->
         <tok val="A(psi)?(s|n|PSIS)"/>
         <lm>
            <l>Apsus</l>
            <m cert="0.5">n e - p - - - m b</m>
            <m cert="0.5">n e - p - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="Apsin"/>
         <lm>
            <l>Apsus</l>
            <m cert="0.333333333333333333">n e - p - - - m n</m>
            <m cert="0.333333333333333333">n e - s - - - m g</m>
            <m cert="0.333333333333333333">n e - p - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="Apso"/>
         <lm>
            <l>Apsus</l>
            <m cert="0.5">n e - s - - - m b</m>
            <m cert="0.5">n e - s - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="Apsoros"/>
         <lm>
            <l>Apsoros</l>
            <m cert="0.5">n e - s - - - f n</m>
            <m cert="0.5">n e - p - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Apsum"/>
         <lm>
            <l>Apsus</l>
            <m cert="0.5">n e - p - - - m g</m>
            <m cert="0.5">n e - s - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Apsus"/>
         <lm>
            <l>Apsus</l>
            <m>n e - s - - - m n</m>
         </lm>
      </ana>
      <ana><!--Apsyctos apsyctos-->
         <tok val="[aA]psyctos"/>
         <lm>
            <l>apsyctos</l>
            <m cert="0.5">n - - s - - - f n</m>
            <m cert="0.5">n - - p - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="apstuleris"/>
         <lm>
            <l>aufero</l>
            <m cert="0.5">v - 2 s r s a</m>
            <m cert="0.5">v - 2 s t i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="apstulero"/>
         <lm>
            <l>aufero</l>
            <m>v - 1 s t i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="apstineas"/>
         <lm>
            <l>abstineo</l>
            <m>v - 2 s p s a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

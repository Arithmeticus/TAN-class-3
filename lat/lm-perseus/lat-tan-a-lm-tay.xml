   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Tay</tok-starts-with>
      <ana>
         <tok val="Taygeta"/>
         <lm>
            <l>Taygeta</l>
            <m cert="0.333333333333333333">n e - p - - - n n</m>
            <m cert="0.333333333333333333">n e - p - - - n v</m>
            <m cert="0.333333333333333333">n e - p - - - n a</m>
         </lm>
      </ana>
      <ana><!--Taygeta Taygete Taygeteque-->
         <tok val="Tayget(e|a|eque)"/>
         <lm>
            <l>Taygete</l>
            <m cert="0.333333333333333333">n e - s - - - f n</m>
            <m cert="0.333333333333333333">n e - s - - - f b</m>
            <m cert="0.333333333333333333">n e - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--Taygete Taygeteque-->
         <tok val="Taygete(que)?"/>
         <lm>
            <l>Taygetus</l>
            <m>n e - s - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="Taygetenque"/>

   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Imu</tok-starts-with>
      <tok-starts-with>imu</tok-starts-with>
      <tok-starts-with>ímu</tok-starts-with>
      <tok-starts-with>Ímu</tok-starts-with>
      <ana><!--Imum imum imumque-->
         <tok val="[iI]mum(que)?"/>
         <lm cert="0.5">
            <l>imus</l>
            <m cert="0.166666666666666667">a - - s - - - n n</m>
            <m cert="0.166666666666666667">a - - p - - - n g</m>
            <m cert="0.166666666666666667">a - - s - - - n a</m>
            <m cert="0.166666666666666667">a - - s - - - n v</m>
            <m cert="0.166666666666666667">a - - p - - - m g</m>
            <m cert="0.166666666666666667">a - - s - - - m a</m>
         </lm>
         <lm cert="0.5">
            <l>imi</l>
            <m>n - - p - - - m g</m>
         </lm>
      </ana>
      <ana><!--Imus imus imusne imusque ímus Ímus-->
         <tok val="[iIíÍ]mus(ne|que)?"/>
         <lm>
            <l>imus</l>
            <m>a - - s - - - m n</m>
         </lm>
      </ana>
      <ana><!--Imus imus imusne imusque-->
         <tok val="[iI]mus(ne|que)?"/>
         <lm>
            <l>eo</l>
            <m>v - 1 p p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="imula"/>
         <lm>
            <l>imulus</l>
            <m cert="0.166666666666666667">a - - s - - - f n</m>
            <m cert="0.166666666666666667">a - - p - - - n n</m>
            <m cert="0.166666666666666667">a - - s - - - f v</m>
            <m cert="0.166666666666666667">a - - p - - - n a</m>
            <m cert="0.166666666666666667">a - - p - - - n v</m>
            <m cert="0.166666666666666667">a - - s - - - f b</m>
         </lm>
      </ana>
      <ana>
         <tok val="imulus"/>
         <lm>
            <l>imulus</l>
            <m>a - - s - - - m n</m>
         </lm>
      </ana>
   </body>

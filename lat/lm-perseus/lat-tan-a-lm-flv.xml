   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>FLV</tok-starts-with>
      <tok-starts-with>Flv</tok-starts-with>
      <ana>
         <tok val="FLVERE"/>
         <lm>
            <l>fluo</l>
            <m cert="0.25">v - 2 s p m p</m>
            <m cert="0.25">v - - - p n a</m>
            <m cert="0.25">v - 2 s p i p</m>
            <m cert="0.25">v - 2 s f i p</m>
         </lm>
      </ana>
      <ana>
         <tok val="FLVMEN"/>
         <lm>
            <l>flumen</l>
            <m cert="0.333333333333333333">n - - s - - - n a</m>
            <m cert="0.333333333333333333">n - - s - - - n v</m>
            <m cert="0.333333333333333333">n - - s - - - n n</m>
         </lm>
      </ana>
      <ana>
         <tok val="FLVMINI"/>
         <lm>
            <l>flumen</l>
            <m>n - - s - - - n d</m>
         </lm>
      </ana>
      <ana>
         <tok val="FLVO"/>
         <lm>
            <l>fluo</l>
            <m>v - 1 s p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Flvmina"/>
         <lm>
            <l>flumen</l>
            <m cert="0.333333333333333333">n - - p - - - n a</m>
            <m cert="0.333333333333333333">n - - p - - - n n</m>
            <m cert="0.333333333333333333">n - - p - - - n v</m>

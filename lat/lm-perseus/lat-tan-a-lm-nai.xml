          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusnai">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to nai</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-nai.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>NAI</tok-starts-with>
      <tok-starts-with>Nai</tok-starts-with>
      <tok-starts-with>nai</tok-starts-with>
      <ana><!--NAI Nai Nain nai nain-->
         <tok val="[Nn]?(ai)?(n|NAI)?"/>
         <lm>
            <l>nautilus</l>
            <m cert="0.333333333333333333">n - - p - - - m n</m>
            <m cert="0.333333333333333333">n - - p - - - m v</m>
            <m cert="0.333333333333333333">n - - s - - - m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="Naiades"/>
         <lm>
            <l>Naias</l>
            <m cert="0.333333333333333333">n e - p - - - f n</m>
            <m cert="0.333333333333333333">n e - p - - - f v</m>
            <m cert="0.333333333333333333">n e - p - - - f a</m>
         </lm>
      </ana>
      <ana><!--Naiadum Naiadumque Naiadvm-->
         <tok val="Naiad[vu]m(que)?"/>
         <lm>
            <l>Naias</l>
            <m>n e - p - - - f g</m>
         </lm>
      </ana>
      <ana>
         <tok val="Naias"/>
         <lm>
            <l>Naias</l>
            <m>n e - s - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Naice"/>
         <lm>
            <l>Naicus</l>
            <m cert="0.25">a e - s - - - n v</m>
            <m cert="0.25">a e - s - - - m v</m>
            <m cert="0.25">n e - s - - - m v</m>
            <m cert="0.25">d e</m>
         </lm>
      </ana>
      <ana>
         <tok val="Naicus"/>
         <lm>
            <l>Naicus</l>
            <m cert="0.5">n e - s - - - m n</m>
            <m cert="0.5">a e - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Naides"/>
         <lm>
            <l>Naides</l>
            <m>n e - p - - - f n</m>
         </lm>
      </ana>
      <ana><!--Nain Nais nain nais-->
         <tok val="[Nn]ai[sn]"/>
         <lm>
            <l>nautilus</l>
            <m cert="0.5">n - - p - - - m b</m>
            <m cert="0.5">n - - p - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="naias"/>
         <lm>
            <l>Naias</l>
            <m>n - - s - - - f n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

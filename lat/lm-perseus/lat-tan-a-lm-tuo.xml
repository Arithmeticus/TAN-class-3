<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseustuo">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to tuo</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-tuo.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>TUO</tok-starts-with>
      <tok-starts-with>Tuo</tok-starts-with>
      <tok-starts-with>tuo</tok-starts-with>
      <tok-starts-with>tuó</tok-starts-with>
      <tok-starts-with>túo</tok-starts-with>
      <tok-starts-with>Túo</tok-starts-with>
      <tok-starts-with>Tuó</tok-starts-with>
      <ana><!--TUO Tuo Tuon Tuoque tuo tuon tuoque tuost-->
         <tok val="[Tt]?(uo)?(n|que|st|TUO)?"/>
         <lm>
            <l>tuus</l>
            <m cert="0.333333333333333333">p s - s - - - m b</m>
            <m cert="0.333333333333333333">p s - s - - - n b</m>
            <m cert="0.333333333333333333">p s - s - - - m d</m>
         </lm>
      </ana>
      <ana><!--TUO Tuo Tuon Tuoque tuo tuon tuoque tuost tuó túo-->
         <tok val="[Tt]?[u]?[o]?(n|que|st|ó|túo|TUO)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - s - - - n d</m>
         </lm>
      </ana>
      <ana><!--TUORUM Tuorum tuorumque tuorum-->
         <tok val="[tT]?(uorum)?(que|TUORUM)?"/>
         <lm cert="0.5">
            <l>tuor</l>
            <m>n - - p - - - m g</m>
         </lm>
         <lm cert="0.5">
            <l>tuus</l>
            <m cert="0.5">p s - p - - - n g</m>
            <m cert="0.5">p s - p - - - m g</m>
         </lm>
      </ana>
      <ana><!--Tuom Tuomst tuom tuomque tuomst tuóm túom-->
         <tok val="[tT]?[u]?[oó]?[m]?(st|túom|tuomque)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - s - - - m a</m>
         </lm>
      </ana>
      <ana><!--Tuom Tuomst tuom tuomque tuomst-->
         <tok val="[tT]uom(st|que)?"/>
         <lm>
            <l>tuus</l>
            <m cert="0.333333333333333333">p s - s - - - n n</m>
            <m cert="0.333333333333333333">p s - s - - - n a</m>
            <m cert="0.333333333333333333">p s - s - - - n v</m>
         </lm>
      </ana>
      <ana><!--Tuon Tuos Tuosne tuon tuos tuosne tuosque tuost tuosve tuós túos Túosne Tuós-->
         <tok val="[tT]?(u|Tú|Tu|tu)?[o]?[s]?(t|ne|ve|n|s|sque|tuós|túos|Tuós)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - p - - - m a</m>
         </lm>
      </ana>
      <ana><!--Tuor tuor tuorque-->
         <tok val="[tT]uor(que)?"/>
         <lm>
            <l>tuor</l>
            <m cert="0.5">n - - s - - - m n</m>
            <m cert="0.5">n - - s - - - m v</m>
         </lm>
      </ana>
      <ana>
         <tok val="tuoris"/>
         <lm>
            <l>tuor</l>
            <m>n - - s - - - m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="tuorum"/>
         <lm cert="0.5">
            <l>tuor2</l>
            <m>n - - p - - - m g</m>
         </lm>
         <lm cert="0.5">
            <l>tuus</l>
            <m>n - - p - - - n g</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>HAN</tok-starts-with>
      <tok-starts-with>HAn</tok-starts-with>
      <tok-starts-with>Han</tok-starts-with>
      <tok-starts-with>han</tok-starts-with>
      <tok-starts-with>hán</tok-starts-with>
      <ana><!--HANC HAnc Hanc Hancine hanc hancine hancne hancque hánc hancíne Hancne-->
         <tok val="[hH]?(a|HA|há)?(nc)?[ií]?(ne|que|HANC)?"/>
         <lm>
            <l>hic</l>
            <m>p d - s - - - f a</m>
         </lm>
      </ana>
      <ana><!--Han han hanne-->
         <tok val="[hH]an(ne)?"/>
         <lm>
            <l>hic</l>
            <m>p d - p - - - f a</m>
         </lm>
      </ana>
      <ana><!--Han Hane han hane hanne-->
         <tok val="[hH]an[n]?[e]?"/>
         <lm>
            <l>ha</l>
            <m>i</m>
         </lm>
      </ana>
      <ana>
         <tok val="Hannibal"/>
         <lm>
            <l>Hannibal</l>
            <m>n e - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Hannibale"/>
         <lm>
            <l>Hannibal</l>
            <m>n e - s - - - m b</m>
         </lm>
      </ana>
      <ana><!--Hannibalem Hannibalemne Hannibalemque-->
         <tok val="Hannibalem(ne|que)?"/>
         <lm>
            <l>Hannibal</l>
            <m>n e - s - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Hannibales"/>
         <lm>
            <l>Hannibal</l>
            <m cert="0.333333333333333333">n e - p - - - m a</m>
            <m cert="0.333333333333333333">n e - p - - - m n</m>
            <m cert="0.333333333333333333">n e - p - - - m v</m>
         </lm>
      </ana>
      <ana><!--Hannibali Hannibalique-->
         <tok val="Hannibali(que)?"/>
         <lm>
            <l>Hannibal</l>
            <m>n e - s - - - m d</m>
         </lm>
      </ana>
      <ana>
         <tok val="Hannibalis"/>
         <lm>
            <l>Hannibal</l>
            <m>n e - s - - - m g</m>
         </lm>
      </ana>
      <ana><!--Hanno Hannon-->
         <tok val="Hanno[n]?"/>
         <lm>
            <l>Hanno</l>
            <m cert="0.5">n e - s - - - m v</m>
            <m cert="0.5">n e - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="Hannone"/>
         <lm>
            <l>Hanno</l>
            <m>n e - s - - - m b</m>
         </lm>
      </ana>
      <ana><!--Hannonem Hannonemque-->
         <tok val="Hannonem(que)?"/>

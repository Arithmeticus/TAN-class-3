<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusmia">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to mia</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-mia.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>MIA</tok-starts-with>
      <tok-starts-with>MiA</tok-starts-with>
      <tok-starts-with>Mia</tok-starts-with>
      <tok-starts-with>mia</tok-starts-with>
      <ana><!--MIA MiA Mia mia mian miaque-->
         <tok val="[mM]?[i]?[a]?(n|A|MIA|Mia|miaque)?"/>
         <lm cert="0.5">
            <l>mius</l>
            <m cert="0.166666666666666667">a - - s - - - f n</m>
            <m cert="0.166666666666666667">a - - p - - - n n</m>
            <m cert="0.166666666666666667">a - - s - - - f v</m>
            <m cert="0.166666666666666667">a - - p - - - n v</m>
            <m cert="0.166666666666666667">a - - p - - - n a</m>
            <m cert="0.166666666666666667">a - - s - - - f b</m>
         </lm>
         <lm cert="0.5">
            <l>mio</l>
            <m>v - 2 s p m a</m>
         </lm>
      </ana>
      <ana><!--MiAi miae-->
         <tok val="[mM]i(ae|Ai)"/>
         <lm>
            <l>mius</l>
            <m>a - - s - - - f g</m>
         </lm>
      </ana>
      <ana>
         <tok val="miae"/>
         <lm>
            <l>mius</l>
            <m cert="0.333333333333333333">a - - p - - - f v</m>
            <m cert="0.333333333333333333">a - - s - - - f d</m>
            <m cert="0.333333333333333333">a - - p - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="miam"/>
         <lm>
            <l>mius</l>
            <m>a - - s - - - f a</m>
         </lm>
      </ana>
      <ana><!--mian mias-->
         <tok val="mia[sn]"/>
         <lm cert="0.5">
            <l>mio</l>
            <m>v - 2 s p i a</m>
         </lm>
         <lm cert="0.5">
            <l>mius</l>
            <m>a - - p - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="miant"/>
         <lm>
            <l>mio</l>
            <m>v - 3 p p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="miare"/>
         <lm>
            <l>mio</l>
            <m cert="0.333333333333333333">v - - - p n a</m>
            <m cert="0.333333333333333333">v - 2 s p m p</m>
            <m cert="0.333333333333333333">v - 2 s p i p</m>
         </lm>
      </ana>
      <ana>
         <tok val="miat"/>
         <lm>
            <l>mio</l>
            <m>v - 3 s p i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="miata"/>
         <lm>
            <l>mio</l>
            <m cert="0.166666666666666667">v - - p r p p n a</m>
            <m cert="0.166666666666666667">v - - s r p p f b</m>
            <m cert="0.166666666666666667">v - - s r p p f n</m>
            <m cert="0.166666666666666667">v - - s r p p f v</m>
            <m cert="0.166666666666666667">v - - p r p p n v</m>
            <m cert="0.166666666666666667">v - - p r p p n n</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

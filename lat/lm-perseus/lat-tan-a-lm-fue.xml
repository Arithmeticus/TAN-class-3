<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusfue">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to fue</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-fue.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>FUE</tok-starts-with>
      <tok-starts-with>Fue</tok-starts-with>
      <tok-starts-with>fue</tok-starts-with>
      <tok-starts-with>fué</tok-starts-with>
      <ana><!--FUERAT Fuerat fuerat fueratque-->
         <tok val="[fF]?(uerat)?(que|FUERAT)?"/>
         <lm>
            <l>sum</l>
            <m>v - 3 s l i a</m>
         </lm>
      </ana>
      <ana><!--Fue fue-->
         <tok val="[fF]ue"/>
         <lm>
            <l>fue</l>
            <m>i</m>
         </lm>
      </ana>
      <ana><!--Fuerant fuerant-->
         <tok val="[fF]uerant"/>
         <lm>
            <l>sum</l>
            <m>v - 3 p l i a</m>
         </lm>
      </ana>
      <ana><!--Fuere Fuerunt Fueruntne fuere fuerunt fueruntque fuére-->
         <tok val="[Ff]u(ér)?e[r]?(unt)?(ne|que|e|)?"/>
         <lm>
            <l>sum</l>
            <m>v - 3 p r i a</m>
         </lm>
      </ana>
      <ana><!--Fuerim fuerim fuerimque-->
         <tok val="[fF]uerim(que)?"/>
         <lm>
            <l>sum</l>
            <m>v - 1 s r s a</m>
         </lm>
      </ana>
      <ana><!--Fuerint fuerint fuerintne fuerintque-->
         <tok val="[fF]uerint(ne|que)?"/>
         <lm>
            <l>sum</l>
            <m cert="0.5">v - 3 p t i a</m>
            <m cert="0.5">v - 3 p r s a</m>
         </lm>
      </ana>
      <ana><!--Fueris fuerin fueris fuerisne-->
         <tok val="[fF]ueri[s]?[n]?[es]?"/>
         <lm>
            <l>sum</l>
            <m cert="0.5">v - 2 s t i a</m>
            <m cert="0.5">v - 2 s r s a</m>
         </lm>
      </ana>
      <ana><!--Fuerit fuerit fueritne fueritque fueritue fueritve-->
         <tok val="[fF]uerit[q]?[unv]?[e]?"/>
         <lm>
            <l>sum</l>
            <m cert="0.5">v - 3 s r s a</m>
            <m cert="0.5">v - 3 s t i a</m>
         </lm>
      </ana>
      <ana><!--fueram fueramque-->
         <tok val="fueram(que)?"/>
         <lm>
            <l>sum</l>
            <m>v - 1 s l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fueramus"/>
         <lm>
            <l>sum</l>
            <m>v - 1 p l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fueras"/>
         <lm>
            <l>sum</l>
            <m>v - 2 s l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fueratis"/>
         <lm>
            <l>sum</l>
            <m>v - 2 p l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fuerimus"/>
         <lm>
            <l>sum</l>
            <m cert="0.5">v - 1 p t i a</m>
            <m cert="0.5">v - 1 p r s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fueritis"/>
         <lm>
            <l>sum</l>
            <m cert="0.5">v - 2 p r s a</m>
            <m cert="0.5">v - 2 p t i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fuero"/>
         <lm>
            <l>sum</l>
            <m>v - 1 s t i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

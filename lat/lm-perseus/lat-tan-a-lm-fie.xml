<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusfie">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to fie</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-fie.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>FIE</tok-starts-with>
      <tok-starts-with>FIe</tok-starts-with>
      <tok-starts-with>Fie</tok-starts-with>
      <tok-starts-with>fie</tok-starts-with>
      <tok-starts-with>fíe</tok-starts-with>
      <tok-starts-with>Fíe</tok-starts-with>
      <ana><!--FIERENT fierent fierentque-->
         <tok val="(fierent)?(que|FIERENT)?"/>
         <lm>
            <l>fio</l>
            <m>v - 3 p - s a</m>
         </lm>
      </ana>
      <ana><!--FIERI FIeri Fieri Fierique fieri fierine fierique fieriue fierive fierí fíeri-->
         <tok val="[fF]?(i|FI|fi|fí)?(er)?[i]?[q]?[unv]?(e|í|i|FIERI)?"/>
         <lm>
            <l>fio</l>
            <m>v - - - p n p</m>
         </lm>
      </ana>
      <ana><!--Fiebant fiebant-->
         <tok val="[fF]iebant"/>
         <lm>
            <l>fio</l>
            <m>v - 3 p - i a</m>
         </lm>
      </ana>
      <ana><!--Fiebat fiebat fíebat-->
         <tok val="[fF]?(i|fí)ebat"/>
         <lm>
            <l>fio</l>
            <m>v - 3 s - i a</m>
         </lm>
      </ana>
      <ana><!--Fient fient fientque-->
         <tok val="[fF]ient(que)?"/>
         <lm>
            <l>fio</l>
            <m>v - 3 p f i a</m>
         </lm>
      </ana>
      <ana><!--Fieret fieret fieretque fieretue fierét-->
         <tok val="[fF]ier[eé]t[q]?(ue)?"/>
         <lm>
            <l>fio</l>
            <m>v - 3 s - s a</m>
         </lm>
      </ana>
      <ana><!--Fies fien fies-->
         <tok val="[fF]ie[sn]"/>
         <lm>
            <l>fio</l>
            <m>v - 2 s f i a</m>
         </lm>
      </ana>
      <ana><!--Fiet fiet fietque fíet Fíet-->
         <tok val="[fF]?(i|fí|Fí|fi)et(que)?"/>
         <lm>
            <l>fio</l>
            <m>v - 3 s f i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fiebam"/>
         <lm>
            <l>fio</l>
            <m>v - 1 s - i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fiebas"/>
         <lm>
            <l>fio</l>
            <m>v - 2 s - i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fierem"/>
         <lm>
            <l>fio</l>
            <m>v - 1 s - s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fieres"/>
         <lm>
            <l>fio</l>
            <m>v - 2 s - s a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

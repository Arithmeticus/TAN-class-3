      <ana><!--Vix Vixque vix vixque víx ViX-->
         <tok val="[vV]?(i|ví)?[x]?(que|ViX)?"/>
         <lm>
            <l>vix</l>
            <m>d</m>
         </lm>
      </ana>
      <ana><!--Vixere Vixerunt vixere vixerunt vixeruntque-->

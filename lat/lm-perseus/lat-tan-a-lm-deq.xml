<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusdeq">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to deq</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-deq.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Deq</tok-starts-with>
      <tok-starts-with>deq</tok-starts-with>
      <ana><!--Deque deque-->
         <tok val="[dD]eque"/>
         <lm>
            <l>de2</l>
            <m cert="0.5">d</m>
            <m cert="0.5">r</m>
         </lm>
      </ana>
      <ana>
         <tok val="Dequeror"/>
         <lm>
            <l>de_queror</l>
            <m>v - 1 s p i p</m>
         </lm>
      </ana>
      <ana>
         <tok val="dequereris"/>
         <lm>
            <l>de_queror</l>
            <m cert="0.5">v - 2 s p i p</m>
            <m cert="0.5">v - 2 s f i p</m>
         </lm>
      </ana>
      <ana>
         <tok val="dequeri"/>
         <lm>
            <l>de_queror</l>
            <m>v - - - p n p</m>
         </lm>
      </ana>
      <ana>
         <tok val="dequesta"/>
         <lm>
            <l>dequestus</l>
            <m cert="0.166666666666666667">a - - s - - - f n</m>
            <m cert="0.166666666666666667">a - - p - - - n n</m>
            <m cert="0.166666666666666667">a - - s - - - f v</m>
            <m cert="0.166666666666666667">a - - p - - - n a</m>
            <m cert="0.166666666666666667">a - - p - - - n v</m>
            <m cert="0.166666666666666667">a - - s - - - f b</m>
         </lm>
      </ana>
      <ana>
         <tok val="dequestus"/>
         <lm>
            <l>dequestus</l>
            <m>a - - s - - - m n</m>
         </lm>
      </ana>
      <ana>
         <tok val="dequis"/>
         <lm>
            <l>deque</l>
            <m>v - 2 s p i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusaeu">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to aeu</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-aeu.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>AEu</tok-starts-with>
      <tok-starts-with>Aeu</tok-starts-with>
      <tok-starts-with>aeu</tok-starts-with>
      <ana><!--AEui Aeui aeui aeuique-->
         <tok val="[A]?(e|E|ae)ui(que)?"/>
         <lm>
            <l>aevum</l>
            <m>n - - s - - - n g</m>
         </lm>
      </ana>
      <ana><!--AEuis aeuis aeuisque-->
         <tok val="(ae|AE)uis(que)?"/>
         <lm>
            <l>aevum</l>
            <m cert="0.5">n - - p - - - n d</m>
            <m cert="0.5">n - - p - - - n b</m>
         </lm>
      </ana>
      <ana><!--AEuitas aeuitasue-->
         <tok val="(ae|AE)uitas(ue)?"/>
         <lm>
            <l>aevitas</l>
            <m cert="0.5">n - - s - - - f n</m>
            <m cert="0.5">v - 2 s p i a</m>
         </lm>
      </ana>
      <ana><!--AEuo Aeuo aeuo aeuoque-->
         <tok val="[A]?(e|E|ae)uo(que)?"/>
         <lm>
            <l>aevum</l>
            <m cert="0.5">n - - s - - - n d</m>
            <m cert="0.5">n - - s - - - n b</m>
         </lm>
      </ana>
      <ana><!--AEuum Aeuom Aeuum aeuom aeuum aeuumque-->
         <tok val="[A]?[eE]?[u]?[uo]?(m|aeuom|aeuum|aeuumque)"/>
         <lm>
            <l>aevum</l>
            <m cert="0.333333333333333333">n - - s - - - n v</m>
            <m cert="0.333333333333333333">n - - s - - - n a</m>
            <m cert="0.333333333333333333">n - - s - - - n n</m>
         </lm>
      </ana>
      <ana><!--AEuum Aeuum aeuum aeuumque-->
         <tok val="[A]?(e|E|ae)uum(que)?"/>
         <lm>
            <l>aevum</l>
            <m>n - - p - - - n g</m>
         </lm>
      </ana>
      <ana><!--Aeuitatem aeuitatem-->
         <tok val="[aA]euitatem"/>
         <lm>
            <l>aevitas</l>
            <m>n - - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="aeuitates"/>
         <lm>
            <l>aevitas</l>
            <m cert="0.333333333333333333">n - - p - - - f v</m>
            <m cert="0.333333333333333333">n - - p - - - f a</m>
            <m cert="0.333333333333333333">n - - p - - - f n</m>
         </lm>
      </ana>
      <ana>
         <tok val="aeuitatis"/>
         <lm>
            <l>aevitas</l>
            <m cert="0.125">v - 2 p p i a</m>
            <m cert="0.125">v - - p r p p n d</m>
            <m cert="0.125">v - - p r p p f d</m>
            <m cert="0.125">v - - p r p p n b</m>
            <m cert="0.125">v - - p r p p m d</m>
            <m cert="0.125">n - - s - - - f g</m>
            <m cert="0.125">v - - p r p p f b</m>
            <m cert="0.125">v - - p r p p m b</m>
         </lm>
      </ana>
      <ana>
         <tok val="aeuola"/>
         <lm>
            <l>aevolo</l>
            <m>v - 2 s p m a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

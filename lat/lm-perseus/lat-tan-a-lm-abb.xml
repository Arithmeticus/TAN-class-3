   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>ABB</tok-starts-with>
      <tok-starts-with>ABb</tok-starts-with>
      <tok-starts-with>Abb</tok-starts-with>
      <tok-starts-with>abb</tok-starts-with>
      <ana><!--ABBAS Abban Abbas-->
         <tok val="A(bba)?(s|n|BBAS)"/>
         <lm>
            <l>Abba</l>
            <m>n e - p - - - f a</m>
         </lm>
      </ana>
      <ana><!--ABba Abba Abban-->
         <tok val="A[bB]ba[n]?"/>
         <lm>
            <l>Abba</l>
            <m cert="0.333333333333333333">n e - s - - - f n</m>
            <m cert="0.333333333333333333">n e - s - - - f b</m>
            <m cert="0.333333333333333333">n e - s - - - f v</m>

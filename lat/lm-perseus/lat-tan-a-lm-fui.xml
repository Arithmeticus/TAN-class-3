<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusfui">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to fui</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-perseus/lat-tan-a-lm-fui.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>FUI</tok-starts-with>
      <tok-starts-with>Fui</tok-starts-with>
      <tok-starts-with>fui</tok-starts-with>
      <tok-starts-with>fúi</tok-starts-with>
      <tok-starts-with>fuí</tok-starts-with>
      <ana><!--FUIT Fuit Fuitne Fuitque fuit fuitne fuitque fuít fúit-->
         <tok val="[fF]?[u]?[ií]?[t]?(qu|n)?(e|fúit|FUIT)?"/>
         <lm>
            <l>sum</l>
            <m>v - 3 s r i a</m>
         </lm>
      </ana>
      <ana><!--Fui fui fúi-->
         <tok val="[fF]?(u|fú)i"/>
         <lm>
            <l>sum</l>
            <m>v - 1 s r i a</m>
         </lm>
      </ana>
      <ana><!--Fuimus fuimus fuimusque fuimusve-->
         <tok val="[fF]uimus(ve|que)?"/>
         <lm>
            <l>sum</l>
            <m>v - 1 p r i a</m>
         </lm>
      </ana>
      <ana><!--Fuisse fuisse fuisseque fuísse fúisse-->
         <tok val="[fF]?[u]?(i|í|fúi|fui)sse(que)?"/>
         <lm>
            <l>sum</l>
            <m>v - - - r n a</m>
         </lm>
      </ana>
      <ana><!--Fuissem fuissem-->
         <tok val="[fF]uissem"/>
         <lm>
            <l>sum</l>
            <m>v - 1 s l s a</m>
         </lm>
      </ana>
      <ana><!--Fuissemus fuissemus-->
         <tok val="[fF]uissemus"/>
         <lm>
            <l>sum</l>
            <m>v - 1 p l s a</m>
         </lm>
      </ana>
      <ana><!--Fuissent fuissent fuíssent-->
         <tok val="[fF]u[ií]ssent"/>
         <lm>
            <l>sum</l>
            <m>v - 3 p l s a</m>
         </lm>
      </ana>
      <ana><!--Fuisset fuisset fuissetque-->
         <tok val="[fF]uisset(que)?"/>
         <lm>
            <l>sum</l>
            <m>v - 3 s l s a</m>
         </lm>
      </ana>
      <ana><!--Fuisti fuisti fuistin fuistique fuísti-->
         <tok val="[fF]u[ií]sti(n|que)?"/>
         <lm>
            <l>sum</l>
            <m>v - 2 s r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fuisses"/>
         <lm>
            <l>sum</l>
            <m>v - 2 s l s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="fuissetis"/>
         <lm>
            <l>sum</l>
            <m>v - 2 p l s a</m>
         </lm>
      </ana>
      <ana><!--fuistin fuistis fuistisne fuístis-->
         <tok val="fu[ií]sti[s]?[n]?[es]?"/>
         <lm>
            <l>sum</l>
            <m>v - 2 p r i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

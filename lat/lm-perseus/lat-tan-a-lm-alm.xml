<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseusalm">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to alm</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-alm.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>ALM</tok-starts-with>
      <tok-starts-with>Alm</tok-starts-with>
      <tok-starts-with>alm</tok-starts-with>
      <ana><!--ALME Alme alme-->
         <tok val="[aA]?(lme|ALME)"/>
         <lm>
            <l>almus</l>
            <m cert="0.5">d</m>
            <m cert="0.5">a - - s - - - m v</m>
         </lm>
      </ana>
      <ana><!--ALME Alme alme almum-->
         <tok val="[aA]?(lm)?(e|um|ALME)"/>
         <lm>
            <l>almus</l>
            <m>a - - s - - - n v</m>
         </lm>
      </ana>
      <ana><!--ALMVS Almus almus-->
         <tok val="[aA]?(lmus|ALMVS)"/>
         <lm>
            <l>almus</l>
            <m>a - - s - - - m n</m>
         </lm>
      </ana>
      <ana><!--Alma alma almaque-->
         <tok val="[aA]lma(que)?"/>
         <lm>
            <l>almus</l>
            <m cert="0.166666666666666667">a - - s - - - f n</m>
            <m cert="0.166666666666666667">a - - p - - - n n</m>
            <m cert="0.166666666666666667">a - - s - - - f v</m>
            <m cert="0.166666666666666667">a - - p - - - n v</m>
            <m cert="0.166666666666666667">a - - p - - - n a</m>
            <m cert="0.166666666666666667">a - - s - - - f b</m>
         </lm>
      </ana>
      <ana><!--Almae almae-->
         <tok val="[aA]lmae"/>
         <lm>
            <l>almus</l>
            <m cert="0.25">a - - p - - - f v</m>
            <m cert="0.25">a - - p - - - f n</m>
            <m cert="0.25">a - - s - - - f d</m>
            <m cert="0.25">a - - s - - - f g</m>
         </lm>
      </ana>
      <ana><!--Almam almam-->
         <tok val="[aA]lmam"/>
         <lm>
            <l>almus</l>
            <m>a - - s - - - f a</m>
         </lm>
      </ana>
      <ana><!--Almo Almon almo almost-->
         <tok val="[Aa]lmo(n|st)?"/>
         <lm>
            <l>almus</l>
            <m cert="0.25">a - - s - - - n b</m>
            <m cert="0.25">a - - s - - - m d</m>
            <m cert="0.25">a - - s - - - n d</m>
            <m cert="0.25">a - - s - - - m b</m>
         </lm>
      </ana>
      <ana><!--Almo Almon-->
         <tok val="Almo[n]?"/>
         <lm>
            <l>Almo</l>
            <m cert="0.5">n e - s - - - m v</m>
            <m cert="0.5">n e - s - - - m n</m>
         </lm>
      </ana>
      <ana><!--Almon almos almost-->
         <tok val="[aA]lmo[s]?[tn]?"/>
         <lm>
            <l>almus</l>
            <m>a - - p - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Almone"/>
         <lm>
            <l>Almo</l>
            <m>n e - s - - - m b</m>
         </lm>
      </ana>
      <ana>
         <tok val="Almonem"/>
         <lm>
            <l>Almo</l>
            <m>n e - s - - - m a</m>
         </lm>
      </ana>
      <ana>
         <tok val="Almonis"/>
         <lm>
            <l>Almo</l>
            <m>n e - s - - - m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="almi"/>
         <lm>
            <l>almus</l>
            <m cert="0.25">a - - s - - - m g</m>
            <m cert="0.25">a - - p - - - m v</m>
            <m cert="0.25">a - - p - - - m n</m>
            <m cert="0.25">a - - s - - - n g</m>
         </lm>
      </ana>
      <ana>
         <tok val="almis"/>
         <lm>
            <l>almus</l>
            <m cert="0.166666666666666667">a - - p - - - n d</m>
            <m cert="0.166666666666666667">a - - p - - - f b</m>
            <m cert="0.166666666666666667">a - - p - - - f d</m>
            <m cert="0.166666666666666667">a - - p - - - m d</m>
            <m cert="0.166666666666666667">a - - p - - - n b</m>
            <m cert="0.166666666666666667">a - - p - - - m b</m>
         </lm>
      </ana>
      <ana>
         <tok val="almum"/>
         <lm>
            <l>almus</l>
            <m cert="0.2">a - - s - - - n n</m>
            <m cert="0.2">a - - p - - - n g</m>
            <m cert="0.2">a - - s - - - n a</m>
            <m cert="0.2">a - - p - - - m g</m>
            <m cert="0.2">a - - s - - - m a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

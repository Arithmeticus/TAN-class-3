<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseussuo">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to suo</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-suo.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>SUO</tok-starts-with>
      <tok-starts-with>Suo</tok-starts-with>
      <tok-starts-with>suo</tok-starts-with>
      <tok-starts-with>suó</tok-starts-with>
      <tok-starts-with>súo</tok-starts-with>
      <ana><!--SUO Suo Suoque suo suomet suon suone suoque-->
         <tok val="[sS]?(uo)?[n]?(e|que|met|SUO)?"/>
         <lm>
            <l>suo</l>
            <m>v - 1 s p i a</m>
         </lm>
      </ana>
      <ana><!--SUO Suo Suoque suo suon suone suoque suopte-->
         <tok val="[sS]?(uo)?[n]?(e|que|pte|SUO)?"/>
         <lm>
            <l>suus</l>
            <m cert="0.5">p s - s - - - m b</m>
            <m cert="0.5">p s - s - - - n b</m>
         </lm>
      </ana>
      <ana><!--SUO Suo Suoque suo suon suone suoque suó súo-->
         <tok val="[sS]?[u]?[o]?[n]?(e|que|ó|súo|SUO)?"/>
         <lm>
            <l>suus</l>
            <m>p s - s - - - n d</m>
         </lm>
      </ana>
      <ana><!--SUO Suo Suoque suo suon suone suoque-->
         <tok val="[sS]?(uo)?[n]?(e|que|SUO)?"/>
         <lm>
            <l>suus</l>
            <m>p s - s - - - m d</m>
         </lm>
      </ana>
      <ana><!--Suom Suomque suom-->
         <tok val="[sS]uom(que)?"/>
         <lm>
            <l>suus</l>
            <m cert="0.25">p s - s - - - m a</m>
            <m cert="0.25">p s - s - - - n n</m>
            <m cert="0.25">p s - s - - - n a</m>
            <m cert="0.25">p s - s - - - n v</m>
         </lm>
      </ana>
      <ana><!--Suorum suorum suorumque suorumue-->
         <tok val="[sS]uorum[q]?(ue)?"/>
         <lm>
            <l>suus</l>
            <m cert="0.5">p s - p - - - n g</m>
            <m cert="0.5">p s - p - - - m g</m>
         </lm>
      </ana>
      <ana><!--Suos suon suos suosque-->
         <tok val="[sS]uo(s|n|sque)"/>
         <lm>
            <l>suus</l>
            <m>p s - p - - - m a</m>
         </lm>
      </ana>
      <ana><!--Suouetaurilia Suovetaurilia suovetaurilia suouetaurilia-->
         <tok val="[Ss]uo[vu]etaurilia"/>
         <lm>
            <l>suovetaurilia</l>
            <m cert="0.333333333333333333">n - - p - - - n a</m>
            <m cert="0.333333333333333333">n - - p - - - n n</m>
            <m cert="0.333333333333333333">n - - p - - - n v</m>
         </lm>
      </ana>
      <ana><!--suouetaurilibus suovetaurilibus-->
         <tok val="suo[vu]etaurilibus"/>
         <lm>
            <l>suovetaurilia</l>
            <m cert="0.5">n - - p - - - n d</m>
            <m cert="0.5">n - - p - - - n b</m>
         </lm>
      </ana>
      <ana>
         <tok val="SUOS"/>
         <lm>
            <l>suus</l>
            <m>a - - p - - - m a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

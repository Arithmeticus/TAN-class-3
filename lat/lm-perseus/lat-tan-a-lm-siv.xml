<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseussiv">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to siv</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-siv.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>Siv</tok-starts-with>
      <tok-starts-with>siv</tok-starts-with>
      <ana><!--Sive sive sivest-->
         <tok val="[sS]ive(st)?"/>
         <lm>
            <l>sive</l>
            <m>c</m>
         </lm>
      </ana>
      <ana><!--Sivere sivere siverunt-->
         <tok val="[sS]iver(e|unt)"/>
         <lm>
            <l>sino</l>
            <m>v - 3 p r i a</m>
         </lm>
      </ana>
      <ana><!--Sivero sivero-->
         <tok val="[sS]ivero"/>
         <lm>
            <l>sino</l>
            <m>v - 1 s t i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="siveram"/>
         <lm>
            <l>sino</l>
            <m>v - 1 s l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="siverat"/>
         <lm>
            <l>sino</l>
            <m>v - 3 s l i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="siverint"/>
         <lm>
            <l>sino</l>
            <m cert="0.5">v - 3 p t i a</m>
            <m cert="0.5">v - 3 p r s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="siveris"/>
         <lm>
            <l>sino</l>
            <m cert="0.5">v - 2 s t i a</m>
            <m cert="0.5">v - 2 s r s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="sivi"/>
         <lm>
            <l>sino</l>
            <m>v - 1 s r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="sivissent"/>
         <lm>
            <l>sino</l>
            <m>v - 3 p l s a</m>
         </lm>
      </ana>
      <ana>
         <tok val="sivistis"/>
         <lm>
            <l>sino</l>
            <m>v - 2 p r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="sivit"/>
         <lm>
            <l>sino</l>
            <m>v - 3 s r i a</m>
         </lm>
      </ana>
      <ana>
         <tok val="siverit"/>
         <lm>
            <l>sino</l>
            <m cert="0.5">v - 3 s r s a</m>
            <m cert="0.5">v - 3 s t i a</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>

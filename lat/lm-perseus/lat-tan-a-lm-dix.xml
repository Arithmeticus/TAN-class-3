      <ana><!--DIXERUNT DIXERVNT Dixere Dixerunt dixere dixerunt dixeruntque-->
         <tok val="(DIXER)?[VU]?(NT|dixerunt|Dixerunt|dixeruntque|dixere|Dixere)"/>
         <lm>
            <l>dico2</l>
            <m>v - 3 p r i a</m>
         </lm>
      </ana>
      <ana><!--DIXISSE Dixe Dixisse dixe dixisse-->
         <tok val="[dD]?(ix)?(iss)?(e|DIXISSE)"/>
         <lm>
            <l>dico2</l>
            <m>v - - - r n a</m>
         </lm>
      </ana>
      <ana><!--DIXIT Dixit Dixitne Dixitque dixit dixitne dixitque-->
         <tok val="[dD]?(ixit)?(qu|n)?(e|DIXIT)?"/>
         <lm>
            <l>dico2</l>
            <m>v - 3 s r i a</m>
         </lm>
      </ana>
      <ana><!--DIxi Dixi Dixin Dixine dixi dixin dixique dixist díxi-->

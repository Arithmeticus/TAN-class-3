            <l>anonymos</l>
            <m>n - - p - - - f g</m>
         </lm>
      </ana>
      <ana><!--Anonymos anonymos-->
         <tok val="[aA]nonymos"/>
         <lm>
            <l>anonymos</l>
            <m cert="0.5">n - - s - - - f n</m>
            <m cert="0.5">n - - p - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="anodynon"/>
         <lm>
            <l>anodynon</l>
            <m cert="0.5">n - - s - - - n d</m>
            <m cert="0.5">n - - s - - - n b</m>

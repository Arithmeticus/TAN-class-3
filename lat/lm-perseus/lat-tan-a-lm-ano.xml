         </lm>
      </ana>
      <ana>
         <tok val="anonymum"/>
         <lm>
            <l>anonymos</l>
            <m>n - - s - - - f a</m>
         </lm>
      </ana>
      <ana>
         <tok val="anorum"/>
         <lm>
            <l>anus</l>
            <m>n - - p - - - m g</m>
         </lm>
      </ana>
      <ana>
         <tok val="anouo"/>
         <lm>
            <l>anovo</l>
            <m>v - 1 s p i a</m>

            <m cert="0.25">n - - p - - - f n</m>
            <m cert="0.25">n - - s - - - f d</m>
         </lm>
      </ana>
      <ana><!--Anon anon anos-->
         <tok val="[aA]no[ns]"/>
         <lm>
            <l>anus</l>
            <m>n - - p - - - m a</m>
         </lm>
      </ana>
      <ana><!--Anonyme anonyme-->
         <tok val="[aA]nonyme"/>
         <lm>
            <l>anonymos</l>
            <m>n - - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--Anonymi anonymi-->
         <tok val="[aA]nonymi"/>

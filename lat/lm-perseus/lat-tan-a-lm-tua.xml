<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../../../TAN-2018/schemas/TAN-A-lm.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TAN-A-lm xmlns="tag:textalign.net,2015:ns"
          TAN-version="2018"
          id="tag:kalvesmaki.com,2015:tan-a-lm:lat:perseustua">
   <head>
      <name>Perseus lexico-morphological permutations devoted exclusively to tua</name>
      <master-location href="https://raw.githubusercontent.com/Arithmeticus/TAN-lexico-morphology/master/lat/lm-lm-perseus-2018-03-24/lat-tan-a-lm-tua.xml"/>
      <license which="Attribution-ShareAlike 3.0 Unported"/>
      <licensor who="perseus"/>
      <key>
         <IRI>tag:kalvesmaki.com,2014:tan-key:lm</IRI>
         <name>General keywords associated with Kalvesmaki's collection of lexico-morphology files</name>
         <location href="../../lm.TAN-key.xml" when-accessed="2018-03-24"/>
      </key>
      <definitions>
         <lexicon xml:id="LS" which="Lewis and Short"/>
         <morphology xml:id="perseus-dik" which="Perseus Latin morphology"/>
         <organization xml:id="perseus" which="Perseus Project"/>
         <person xml:id="dik" which="Helma Dik"/>
         <person xml:id="kalvesmaki" which="Joel Kalvesmaki"/>
         <role which="creator" xml:id="creator"/>
         <role which="sponsor" xml:id="sponsor"/>
         <role which="editor" xml:id="editor"/>
      </definitions>
      <resp who="perseus dik" roles="creator"/>
      <resp who="perseus" roles="sponsor"/>
      <resp who="kalvesmaki" roles="editor"/>
      <change when="2018-01-11" who="kalvesmaki">Started template file.</change>
      <change who="kalvesmaki" when="2018-03-24">Cleaned file for publication.</change>
   </head>
   <body lexicon="LS" morphology="perseus-dik">
      <for-lang>lat</for-lang>
      <tok-starts-with>TUA</tok-starts-with>
      <tok-starts-with>Tua</tok-starts-with>
      <tok-starts-with>tua</tok-starts-with>
      <tok-starts-with>tuá</tok-starts-with>
      <tok-starts-with>túa</tok-starts-with>
      <ana><!--TUAE Tuae Tuaen tuae tuaeque tuaest-->
         <tok val="[Tt]?(uae)?(n|st|que|TUAE)?"/>
         <lm>
            <l>tuus</l>
            <m cert="0.333333333333333333">p s - p - - - f v</m>
            <m cert="0.333333333333333333">p s - p - - - f n</m>
            <m cert="0.333333333333333333">p s - s - - - f d</m>
         </lm>
      </ana>
      <ana><!--TUAE Tuae Tuaen tuae tuaeque tuaest tuaique tuaé-->
         <tok val="[Tt]?(ua)?(iqu)?[e]?(n|que|st|é|TUAE)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - s - - - f g</m>
         </lm>
      </ana>
      <ana><!--TUAM Tuam Tuamque tuam tuamne tuamque tuamst tuamve tuám túam tuámst-->
         <tok val="[tT]?[u]?[aá]?[m]?(qu|v|n)?(e|st|túam|TUAM)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - s - - - f a</m>
         </lm>
      </ana>
      <ana><!--Tua Tuan Tuane Tuast tua tuan tuane tuaque tuast-->
         <tok val="[Tt]ua[n]?(e|st|que)?"/>
         <lm>
            <l>tuus</l>
            <m cert="0.2">p s - p - - - n n</m>
            <m cert="0.2">p s - s - - - f n</m>
            <m cert="0.2">p s - p - - - n v</m>
            <m cert="0.2">p s - s - - - f b</m>
            <m cert="0.2">p s - s - - - f v</m>
         </lm>
      </ana>
      <ana><!--Tua Tuan Tuane Tuast tua tuan tuane tuaque tuast tuá túa-->
         <tok val="[Tt]?[u]?[a]?[n]?(e|st|que|á|túa)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - p - - - n a</m>
         </lm>
      </ana>
      <ana><!--Tuan Tuas tuan tuas tuasne tuasque tuasue tuás-->
         <tok val="[tT]u[a]?[s]?[q]?[un]?(e|n|s|ás)"/>
         <lm>
            <l>tuus</l>
            <m>p s - p - - - f a</m>
         </lm>
      </ana>
      <ana><!--Tuarum tuarum tuarumque-->
         <tok val="[tT]uarum(que)?"/>
         <lm>
            <l>tuus</l>
            <m>p s - p - - - f g</m>
         </lm>
      </ana>
      <ana><!--Tuatim tuatim-->
         <tok val="[tT]uatim"/>
         <lm>
            <l>tuatim</l>
            <m>d</m>
         </lm>
      </ana>
   </body>
</TAN-A-lm>
